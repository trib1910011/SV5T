{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n// import config from \"@/config\";\nimport axios from \"axios\";\nexport default {\n  name: \"CreateForm\",\n\n  data() {\n    return {\n      student: [],\n      spell: [],\n      standard: [],\n      gpa: \"\",\n      drl: \"\",\n      more: \"\"\n    };\n  },\n\n  created() {\n    this.showSpell();\n    this.getStandard();\n    this.showInforStudent();\n  },\n\n  methods: {\n    async showSpell() {\n      const data = await axios.get(\"http://localhost:3000/spell/get-all-spell\");\n      const arr_data = data.data.findSpell;\n      const arr_result = arr_data.map((item, index) => {\n        return {\n          id: item._id,\n          stt: index + 1,\n          spellname: item.spellname,\n          name: item.name,\n          start: item.start,\n          end: item.end,\n          status: item.status ? 'Bắt đầu' : 'Kết thúc'\n        };\n      });\n      this.spell = arr_result;\n    },\n\n    async handleAssess() {\n      var arr_standard = [];\n      let DOM_Checkbox_standart = document.querySelectorAll(\".standard_check_box\");\n      DOM_Checkbox_standart.forEach(item => {\n        if (item.checked) {\n          arr_standard.push(item.dataset.standard);\n        }\n      });\n      var arr_standard1 = [];\n      let DOM_Checkbox_standart1 = document.querySelectorAll(\".standard_check\");\n      DOM_Checkbox_standart1.forEach(item => {\n        if (item.checked) {\n          arr_standard1.push(item.dataset.standard1);\n        }\n      });\n      let spell_name_choose = \"\";\n      this.spell.forEach(item => {\n        if (item.status === \"Bắt đầu\") {\n          spell_name_choose = item.name;\n        }\n      });\n      const ob = {\n        studentId: this.student[0].id,\n        username: this.username,\n        name: this.name,\n        major: this.student[0].major,\n        course: this.student[0].course,\n        spellname: spell_name_choose,\n        gpa: this.gpa,\n        drl: this.drl,\n        standard: arr_standard,\n        standard1: arr_standard1,\n        more: this.more\n      };\n      const token = localStorage.getItem(\"token\");\n      const res = await axios.post(\"http://localhost:3000/student/register-form\", ob, {\n        headers: {\n          Authorization: \"Bearer \" + token\n        }\n      });\n      this.$store.commit(\"SET_TOAST\", {\n        message: res.data.message,\n        isSuccess: res.data.status\n      });\n      this.$parent.showForm(); // .then((data) => {\n      //     console.log(data)\n      // }).catch((e) => {\n      //     console.log(e)\n      // })\n      //Sau khi thoát khỏi form\n\n      DOM_Checkbox_standart.forEach(item => {\n        item.checked = false;\n      });\n      this.gpa = \"\";\n      this.drl = \"\";\n      this.more = \"\";\n    },\n\n    cancelForm() {\n      let DOM_Checkbox_standart = document.querySelectorAll(\".standard_check_box\");\n      DOM_Checkbox_standart.forEach(item => {\n        item.checked = false;\n      });\n      this.gpa = \"\";\n      this.drl = \"\";\n      this.more = \"\";\n    },\n\n    async getStandard() {\n      const token = localStorage.getItem(\"token\");\n      const data = await axios.get(\"http://localhost:3000/student/get-standard\", {\n        headers: {\n          Authorization: \"Bearer \" + token\n        }\n      });\n      const arr_data = data.data.standards;\n      const arr_result = arr_data.map((item, index) => {\n        return {\n          id: item._id,\n          stt: index + 1,\n          codeStandard: item.codeStandard,\n          categoryName: item.categoryName,\n          contentStandard: item.contentStandard,\n          status: item.status ? 'Hiện' : 'Ẩn',\n          check: item.check\n        };\n      });\n      this.standards = arr_result;\n    },\n\n    async showInforStudent() {\n      const token = localStorage.getItem(\"token\");\n      const data = await axios.post(\"http://localhost:3000/student/get-student-infor\", {\n        token\n      }, {\n        headers: {\n          Authorization: \"Bearer \" + token\n        }\n      });\n      const arr_data = data.data.result;\n      const arr_result = [arr_data].map((item, index) => {\n        return {\n          id: item.studentId,\n          urlAvatar: item.urlAvatar,\n          stt: index + 1,\n          username: item.username,\n          name: item.name,\n          major: item.major,\n          course: item.course\n        };\n      });\n      this.student = arr_result;\n    }\n\n  }\n};","map":{"version":3,"mappings":";AAiLA;AACA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAI,EAAE,YADK;;EAGXC,IAAI,GAAG;IACH,OAAO;MACHC,OAAO,EAAE,EADN;MAEHC,KAAK,EAAE,EAFJ;MAGHC,QAAQ,EAAE,EAHP;MAIHC,GAAG,EAAE,EAJF;MAKHC,GAAG,EAAE,EALF;MAMHC,IAAI,EAAE;IANH,CAAP;EAQH,CAZU;;EAaXC,OAAO,GAAG;IACN,KAAKC,SAAL;IACA,KAAKC,WAAL;IACA,KAAKC,gBAAL;EACH,CAjBU;;EAkBXC,OAAO,EAAE;IACL,MAAMH,SAAN,GAAkB;MACd,MAAMR,IAAG,GAAI,MAAMF,KAAK,CAACc,GAAN,CAAU,2CAAV,CAAnB;MAGA,MAAMC,QAAO,GAAIb,IAAI,CAACA,IAAL,CAAUc,SAA3B;MAEA,MAAMC,UAAS,GAAIF,QAAQ,CAACG,GAAT,CAAa,CAACC,IAAD,EAAOC,KAAP,KAAiB;QAC7C,OAAO;UACHC,EAAE,EAAEF,IAAI,CAACG,GADN;UAEHC,GAAG,EAAEH,KAAI,GAAI,CAFV;UAGHI,SAAS,EAAEL,IAAI,CAACK,SAHb;UAIHvB,IAAI,EAAEkB,IAAI,CAAClB,IAJR;UAKHwB,KAAK,EAAEN,IAAI,CAACM,KALT;UAMHC,GAAG,EAAEP,IAAI,CAACO,GANP;UAOHC,MAAM,EAAER,IAAI,CAACQ,MAAL,GAAc,SAAd,GAA0B;QAP/B,CAAP;MAWH,CAZkB,CAAnB;MAcA,KAAKvB,KAAL,GAAaa,UAAb;IAEH,CAvBI;;IAwBL,MAAMW,YAAN,GAAqB;MACjB,IAAIC,YAAW,GAAI,EAAnB;MACA,IAAIC,qBAAoB,GAAIC,QAAQ,CAACC,gBAAT,CAA0B,qBAA1B,CAA5B;MAEAF,qBAAqB,CAACG,OAAtB,CAA+Bd,IAAD,IAAU;QACpC,IAAIA,IAAI,CAACe,OAAT,EAAkB;UACdL,YAAY,CAACM,IAAb,CAAkBhB,IAAI,CAACiB,OAAL,CAAa/B,QAA/B;QACJ;MACH,CAJD;MAKA,IAAIgC,aAAY,GAAI,EAApB;MACA,IAAIC,sBAAqB,GAAIP,QAAQ,CAACC,gBAAT,CAA0B,iBAA1B,CAA7B;MAEAM,sBAAsB,CAACL,OAAvB,CAAgCd,IAAD,IAAU;QACrC,IAAIA,IAAI,CAACe,OAAT,EAAkB;UACdG,aAAa,CAACF,IAAd,CAAmBhB,IAAI,CAACiB,OAAL,CAAaG,SAAhC;QACJ;MACH,CAJD;MAOA,IAAIC,iBAAgB,GAAI,EAAxB;MACA,KAAKpC,KAAL,CAAW6B,OAAX,CAAoBd,IAAD,IAAU;QAC