{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport config from \"@/config/index\";\nexport default {\n  name: \"CreateSpell\",\n\n  data() {\n    return {\n      spellname: \"\",\n      name: \"\",\n      start: \"\",\n      end: \"\",\n      status: true,\n      account: [],\n      standards: [],\n      arrTeacherChoose: []\n    };\n  },\n\n  created() {\n    this.getAllAccount(), this.getStandard();\n  },\n\n  methods: {\n    findInfo() {\n      let chooseItem = document.querySelector('.inputChange').value;\n      const arr_account = this.account;\n      const arr_result = [];\n      arr_account.forEach((item, index) => {\n        item.name == chooseItem && item.rule == 'TEACHER' ? arr_result.push({\n          id: item._id,\n          stt: index + 1,\n          rule: item.rule,\n          name: item.name\n        }) : '';\n      });\n      this.account = arr_result;\n    },\n\n    addTeacher() {\n      var arr_teacher = [];\n      let DOM_Checkbox_teacher = document.querySelectorAll(\".allocate\");\n      DOM_Checkbox_teacher.forEach(item => {\n        if (item.checked) {\n          arr_teacher.push(item.dataset.allocate);\n        }\n      });\n    },\n\n    async handleCreateSpell() {\n      var arr_teacher = [];\n      let DOM_Checkbox_teacher = document.querySelectorAll(\".allocate\");\n      DOM_Checkbox_teacher.forEach(item => {\n        if (item.checked) {\n          arr_teacher.push(item.dataset.allocate);\n        }\n      });\n      var arr_structure = [];\n      let DOM_Checkbox_structure = document.querySelectorAll(\".standard_check_box\");\n      DOM_Checkbox_structure.forEach(item => {\n        if (item.checked) {\n          arr_structure.push(item.dataset.structure);\n        }\n      });\n      const res = await axios.post(`${config.domain}/spell/create-spell`, {\n        spellname: this.spellname.trim(),\n        name: this.name.trim(),\n        start: this.start,\n        end: this.end,\n        status: this.status,\n        allocate: arr_teacher,\n        structure: arr_structure\n      });\n      this.$store.commit(\"SET_TOAST\", {\n        message: res.data.message,\n        isSuccess: res.data.status\n      });\n\n      if (res.data.status) {\n        this.$store.commit(\"ADD_SPELL_CURRENT\", {\n          _id: res.data.newSpell._id,\n          spellname: this.spellname,\n          name: this.name,\n          start: this.start,\n          end: this.end,\n          status: this.status,\n          allocate: arr_teacher,\n          structure: arr_structure\n        });\n        this.spellname = '';\n        this.name = '';\n        this.start = '';\n        this.end = '';\n        DOM_Checkbox_teacher.forEach(item => {\n          item.checked = false;\n        });\n        DOM_Checkbox_structure.forEach(item => {\n          item.checked = false;\n        });\n      }\n\n      this.$parent.showSpell();\n    },\n\n    async getAllAccount() {\n      const token = localStorage.getItem(\"token\");\n      const data = await axios.get(\"http://localhost:3000/ministry/get-all-account\", {\n        headers: {\n          Authorization: \"Bearer \" + token\n        }\n      });\n      const arr_data = data.data.result;\n      const arr_result = arr_data.map((item, index) => {\n        return {\n          id: item._id,\n          stt: index + 1,\n          rule: item.rule,\n          name: item.name\n        };\n      });\n      this.account = arr_result;\n    },\n\n    async getStandard() {\n      const token = localStorage.getItem(\"token\");\n      const data = await axios.get(\"http://localhost:3000/ministry/get-standard\", {\n        headers: {\n          Authorization: \"Bearer \" + token\n        }\n      });\n      const arr_data = data.data.standards;\n      const arr_result = arr_data.map((item, index) => {\n        return {\n          id: item._id,\n          stt: index + 1,\n          codeStandard: item.codeStandard,\n          categoryName: item.categoryName,\n          contentStandard: item.contentStandard,\n          status: item.status ? 'Hiện' : 'Ẩn',\n          check: item.check\n        };\n      });\n      this.standards = arr_result;\n    }\n\n  }\n};","map":{"version":3,"mappings":";AAsLA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,gBAAnB;AACA,eAAe;EACXC,IAAI,EAAE,aADK;;EAEXC,IAAI,GAAG;IACH,OAAO;MACHC,SAAS,EAAE,EADR;MAEHF,IAAI,EAAE,EAFH;MAGHG,KAAK,EAAE,EAHJ;MAIHC,GAAG,EAAE,EAJF;MAKHC,MAAM,EAAE,IALL;MAMHC,OAAO,EAAE,EANN;MAOHC,SAAS,EAAE,EAPR;MASHC,gBAAgB,EAAE;IATf,CAAP;EAWH,CAdU;;EAeXC,OAAO,GAAG;IACN,KAAKC,aAAL,IACI,KAAKC,WAAL,EADJ;EAEH,CAlBU;;EAmBXC,OAAO,EAAE;IACLC,QAAQ,GAAG;MACP,IAAIC,UAAS,GAAIC,QAAQ,CAACC,aAAT,CAAuB,cAAvB,EAAuCC,KAAxD;MAEA,MAAMC,WAAU,GAAI,KAAKZ,OAAzB;MACA,MAAMa,UAAS,GAAI,EAAnB;MACAD,WAAW,CAACE,OAAZ,CAAoB,CAACC,IAAD,EAAOC,KAAP,KAAiB;QACjCD,IAAI,CAACrB,IAAL,IAAac,UAAb,IAA2BO,IAAI,CAACE,IAAL,IAAa,SAAxC,GAAoDJ,UAAU,CAACK,IAAX,CAChD;UACIC,EAAE,EAAEJ,IAAI,CAACK,GADb;UAEIC,GAAG,EAAEL,KAAI,GAAI,CAFjB;UAGIC,IAAI,EAAEF,IAAI,CAACE,IAHf;UAIIvB,IAAI,EAAEqB,IAAI,CAACrB;QAJf,CADgD,CAApD,GAMS,EANT;MAOH,CARD;MAWA,KAAKM,OAAL,GAAea,UAAf;IACH,CAlBI;;IAsBLS,UAAU,GAAG;MACT,IAAIC,WAAU,GAAI,EAAlB;MACA,IAAIC,oBAAmB,GAAIf,QAAQ,CAACgB,gBAAT,CAA0B,WAA1B,CAA3B;MAEAD,oBAAoB,CAACV,OAArB,CAA8BC,IAAD,IAAU;QACnC,IAAIA,IAAI,CAACW,OAAT,EAAkB;UACdH,WAAW,CAACL,IAAZ,CAAiBH,IAAI,CAACY,OAAL,CAAaC,QAA9B;QACJ;MACH,CAJD;IAKH,CA/BI;;IAiCL,MAAMC,iBAAN,GAA0B;MACtB,IAAIN,WAAU,GAAI,EAAlB;MACA,IAAIC,oBAAmB,GAAIf,QAAQ,CAACgB,gBAAT,CAA0B,WAA1B,CAA3B;MAEAD,oBAAoB,CAACV,OAArB,CAA8BC,IAAD,IAAU;QACnC,IAAIA,IAAI,CAACW,OAAT,EAAkB;UACdH,WAAW,CAACL,IAAZ,CAAiBH,IAAI,CAACY,OAAL,CAAaC,QAA9B;QACJ;MACH,CAJD;MAKA,IAAIE,aAAY,GAAI,EAApB;MACA,IAAIC,sBAAqB,GAAItB,QAAQ,CAACgB,gBAAT,CAA0B,qBAA1B,CAA7B;MAEAM,sBAAsB,CAACjB,OAAvB,CAAgCC,IAAD,IAAU;QACrC,IAAIA,IAAI,CAACW,OAAT,EAAkB;UACdI,aAAa,CAACZ,IAAd,CAAmBH,IAAI,CAACY,OAAL,CAAaK,SAAhC;QACJ;MACH,CAJD;MAKA,MAAMC,GAAE,GAAI,MAAMzC,KAAK,CAAC0C,IAAN,CACb,GAAEzC,MAAM,CAAC0C,MAAO,qBADH,EAEd;QACIvC,SAAS,EAAE,KAAKA,SAAL,