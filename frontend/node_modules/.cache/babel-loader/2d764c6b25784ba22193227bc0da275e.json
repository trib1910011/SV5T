{"ast":null,"code":"import InforTeacher from \"@/components/GlobalComponent/InforTeacher.vue\";\nimport TitleStructure from \"@/components/GlobalComponent/TitleStructure.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"FormManage\",\n  components: {\n    InforTeacher,\n    TitleStructure\n  },\n\n  data() {\n    return {\n      spell: [],\n      empty_check: \"\"\n    };\n  },\n\n  created() {\n    this.showSpell();\n  },\n\n  methods: {\n    async showSpell() {\n      const data = await axios.get(\"http://localhost:3000/spell/get-all-spell\");\n      const arr_data = data.data.findSpell;\n      const arr_result = arr_data.map((item, index) => {\n        return {\n          id: item._id,\n          stt: index + 1,\n          spellname: item.spellname,\n          name: item.name,\n          start: item.start,\n          end: item.end,\n          status: item.status ? 'Bắt đầu' : 'Kết thúc'\n        };\n      });\n      var count_empty = 0;\n      arr_result.forEach(item => {\n        if (item.status === \"Kết thúc\") {\n          count_empty++;\n        }\n      });\n\n      if (count_empty === arr_result.length) {\n        this.empty_check = \"Chưa đến đợt xét duyệt\";\n      }\n\n      this.spell = arr_result;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+CA,OAAOA,YAAP,MAAyB,+CAAzB;AACA,OAAOC,cAAP,MAA2B,iDAA3B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAI,EAAE,YADK;EAEXC,UAAU,EAAE;IAAEJ,YAAF;IAAgBC;EAAhB,CAFD;;EAGXI,IAAI,GAAG;IACH,OAAO;MACHC,KAAK,EAAE,EADJ;MAEHC,WAAW,EAAE;IAFV,CAAP;EAIH,CARU;;EASXC,OAAO,GAAG;IACN,KAAKC,SAAL;EACH,CAXU;;EAYXC,OAAO,EAAE;IACL,MAAMD,SAAN,GAAkB;MACd,MAAMJ,IAAG,GAAI,MAAMH,KAAK,CAACS,GAAN,CAAU,2CAAV,CAAnB;MAEA,MAAMC,QAAO,GAAIP,IAAI,CAACA,IAAL,CAAUQ,SAA3B;MAEA,MAAMC,UAAS,GAAIF,QAAQ,CAACG,GAAT,CAAa,CAACC,IAAD,EAAOC,KAAP,KAAiB;QAE7C,OAAO;UACHC,EAAE,EAAEF,IAAI,CAACG,GADN;UAEHC,GAAG,EAAEH,KAAI,GAAI,CAFV;UAGHI,SAAS,EAAEL,IAAI,CAACK,SAHb;UAIHlB,IAAI,EAAEa,IAAI,CAACb,IAJR;UAKHmB,KAAK,EAAEN,IAAI,CAACM,KALT;UAMHC,GAAG,EAAE