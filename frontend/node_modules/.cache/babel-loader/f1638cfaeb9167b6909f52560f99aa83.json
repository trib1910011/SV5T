{"ast":null,"code":"import InforStudent from \"@/components/GlobalComponent/InforStudent.vue\";\nimport TitleStructure from \"../../components/GlobalComponent/TitleStructure.vue\";\nimport axios from \"axios\";\nimport CreateForm from \"@/components/Student/Form/CreateForm.vue\";\nexport default {\n  name: \"FormRegister\",\n  components: {\n    TitleStructure,\n    InforStudent,\n    CreateForm\n  },\n\n  data() {\n    return {\n      spell: [],\n      empty_check: \"\",\n      form: []\n    };\n  },\n\n  created() {\n    this.showSpell(), this.showForm();\n  },\n\n  methods: {\n    async showSpell() {\n      const data = await axios.get(\"http://localhost:3000/spell/get-all-spell\");\n      const arr_data = data.data.findSpell;\n      const arr_result = arr_data.map((item, index) => {\n        return {\n          id: item._id,\n          stt: index + 1,\n          spellname: item.spellname,\n          name: item.name,\n          start: item.start,\n          end: item.end,\n          status: item.status ? 'Bắt đầu' : 'Kết thúc'\n        };\n      });\n      var count_empty = 0;\n      arr_result.forEach(item => {\n        if (item.status === \"Kết thúc\") {\n          count_empty++;\n        }\n      });\n\n      if (count_empty === arr_result.length) {\n        this.empty_check = \"CHƯA ĐẾN ĐỢT ĐĂNG KÝ\";\n      }\n\n      this.spell = arr_result;\n    },\n\n    async showForm() {\n      const token = localStorage.getItem(\"token\");\n      const data1 = await axios.get(\"http://localhost:3000/student/get-form\", {\n        headers: {\n          Authorization: \"Bearer \" + token\n        }\n      });\n      const arr_data1 = data1.data.findForm;\n      const arr_result1 = arr_data1.map((form, index) => {\n        return {\n          id: form._id,\n          stt: index + 1,\n          name: form.spellname,\n          created: form.created,\n          result: form.result\n        };\n      });\n      console.log(arr_result1);\n      this.form = arr_result1;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkGA,OAAOA,YAAP,MAAyB,+CAAzB;AACA,OAAOC,cAAP,MAA2B,qDAA3B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,UAAP,MAAuB,0CAAvB;AACA,eAAe;EACXC,IAAI,EAAE,cADK;EAEXC,UAAU,EAAE;IAAEJ,cAAF;IAAkBD,YAAlB;IAAgCG;EAAhC,CAFD;;EAGXG,IAAI,GAAG;IACH,OAAO;MACHC,KAAK,EAAE,EADJ;MAEHC,WAAW,EAAE,EAFV;MAGHC,IAAI,EAAE;IAHH,CAAP;EAKH,CATU;;EAUXC,OAAO,GAAG;IACN,KAAKC,SAAL,IACI,KAAKC,QAAL,EADJ;EAEH,CAbU;;EAcXC,OAAO,EAAE;IACL,MAAMF,SAAN,GAAkB;MACd,MAAML,IAAG,GAAI,MAAMJ,KAAK,CAACY,GAAN,CAAU,2CAAV,CAAnB;MAEA,MAAMC,QAAO,GAAIT,IAAI,CAACA,IAAL,CAAUU,SAA3B;MAEA,MAAMC,UAAS,GAAIF,QAAQ,CAACG,GAAT,CAAa,CAACC,IAAD,EAAOC,KAAP,KAAiB;QAE7C,OAAO;UACHC,EAAE,EAAEF,IAAI,CAACG,GADN;UAEHC,GAAG,EAAEH,KAAI,GAAI,CAFV;UAGHI,SAAS,EAAEL,IAAI,CAACK,SAHb;UAIHpB,IAAI,EAAEe,IAAI,CAACf,IAJR;UAKHqB,KAAK,EAAEN,IAAI,CAACM,KALT;UAMHC,GAAG,EAAEP,IAAI,CAACO,GANP;UAOHC,MAAM,EAAER,IAAI,CAACQ,MAAL,GAAc,SAAd,GAA0B;QAP/B,CAAP;MAWH,CAbkB,CAAnB;MAeA,IAAIC,WAAU,GAAI,CAAlB;MACAX,UAAU,CAACY,OAAX,CAAoBV,IAAD,IAAU;QACzB,IAAIA,IAAI,CAACQ,MAAL,KAAgB,UAApB,EAAgC;UAC5BC,WAAW;QACf;MACH,CAJD;;MAMA,IAAIA,WAAU,KAAMX,UAAU,CAACa,MAA/B,EAAuC;QACnC,KAAKtB,WAAL,GAAmB,sBAAnB;MACJ;;MAEA,KAAKD,KAAL,GAAaU,UAAb;IACH,CAjCI;;IAkCL,MAAML,QAAN,GAAiB;MACb,MAAMmB,KAAI,GAAIC,YAAY,CAACC,OAAb,CAAqB,OAArB,CAAd;MACA,MAAMC,KAAI,GAAI,MAAMhC,KAAK,CAACY,GAAN,CAAU,wCAAV,EAAoD;QAACqB,OAAO,EAAE;UAC9EC,aAAa,EAAE,YAAYL;QADmD;MAAV,CAApD,CAApB;MAIA,MAAMM,SAAQ,GAAIH,KAAK,CAAC5B,IAAN,CAAWgC,QAA7B;MAEA,MAAMC,WAAU,GAAIF,SAAS,CAACnB,GAAV,CAAc,CAACT,IAAD,EAAOW,KAAP,KAAiB;QAE/C,OAAO;UACHC,EAAE,EAAEZ,IAAI,CAACa,GADN;UAEHC,GAAG,EAAEH,KAAI,GAAI,CAFV;UAGHhB,IAAI,EAAEK,IAAI,CAACe,SAHR;UAIHd,OAAO,EAAED,IAAI,CAACC,OAJX;UAKH8B,MAAM,EAAE/B,IAAI,CAAC+B;QALV,CAAP;MASH,CAXmB,CAApB;MAYAC,OAAO,CAACC,GAAR,CAAYH,WAAZ;MACA,KAAK9B,IAAL,GAAY8B,WAAZ;IACJ;;EAxDK;AAdE,CAAf","names":["InforStudent","TitleStructure","axios","CreateForm","name","components","data","spell","empty_check","form","created","showSpell","showForm","methods","get","arr_data","findSpell","arr_result