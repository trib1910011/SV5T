{"ast":null,"code":"import InforStudent from \"@/components/GlobalComponent/InforStudent.vue\";\nimport TitleStructure from \"../../components/GlobalComponent/TitleStructure.vue\";\nimport axios from \"axios\"; // import CreateForm from \"@/components/Student/Form/CreateForm.vue\";\n\nexport default {\n  name: \"FormRegister\",\n  components: {\n    TitleStructure,\n    InforStudent\n  },\n\n  data() {\n    return {\n      spell: []\n    };\n  },\n\n  created() {\n    this.showSpell();\n  },\n\n  methods: {\n    async showSpell() {\n      const data = await axios.get(\"http://localhost:3000/spell/get-all-spell\");\n      const arr_data = data.data.findSpell;\n      const arr_result = arr_data.map((item, index) => {\n        console.log(item.status);\n        return {\n          id: item._id,\n          stt: index + 1,\n          spellname: item.spellname,\n          name: item.name,\n          start: item.start,\n          end: item.end,\n          status: item.status ? 'Bắt đầu' : 'Kết thúc'\n        };\n      });\n      console.log(arr_result);\n      this.spell = arr_result;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6EA,OAAOA,YAAP,MAAyB,+CAAzB;AACA,OAAOC,cAAP,MAA2B,qDAA3B;AACA,OAAOC,KAAP,MAAkB,OAAlB,C,CACA;;AACA,eAAe;EACXC,IAAI,EAAE,cADK;EAEXC,UAAU,EAAE;IAAEH,cAAF;IAAkBD;EAAlB,CAFD;;EAGXK,IAAI,GAAG;IACH,OAAO;MACHC,KAAK,EAAE;IADJ,CAAP;EAGH,CAPU;;EAQXC,OAAO,GAAG;IACN,KAAKC,SAAL;EACH,CAVU;;EAWXC,OAAO,EAAE;IACL,MAAMD,SAAN,GAAkB;MACd,MAAMH,IAAG,GAAI,MAAMH,KAAK,CAACQ,GAAN,CAAU,2CAAV,CAAnB;MAEA,MAAMC,QAAO,GAAIN,IAAI,CAACA,IAAL,CAAUO,SAA3B;MAEA,MAAMC,UAAS,GAAIF,QAAQ,CAACG,GAAT,CAAa,CAACC,IAAD,EAAOC,KAAP,KAAiB;QAC7CC,OAAO,CAACC,GAAR,CAAYH,IAAI,CAACI,MAAjB;QACI,OAAO;UACPC,EAAE,EAAEL,IAAI,CAACM,GADF;UAEPC,GAAG,EAAEN,KAAI,GAAI,CAFN;UAGPO,SAAS,EAAER,IAAI,CAACQ,SAHT;UAIPpB,IAAI,EAAEY,IAAI,CAACZ,IAJJ;UAKPqB,KAAK,EAAET,IAAI,CAACS,KALL;UAMPC,GAAG,EAAEV,IAAI,CAACU,GANH;UAOPN,MAAM,EAAEJ,IAAI,CAACI,MAAL,GAAc,SAAd,GAA0B;QAP3B,CAAP;MAWP,CAbkB,CAAnB;MAcAF,OAAO,CAACC,GAAR,CAAY