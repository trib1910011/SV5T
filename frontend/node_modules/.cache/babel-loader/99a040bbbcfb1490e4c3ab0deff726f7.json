rong>MSSV: </strong> {{ item.username }}<br>\r\n                            <strong>NGÀNH: </strong> {{ item.major }}<br>\r\n                            <strong>KHÓA: </strong> {{ item.course }}<br>\r\n                            </template>\r\n                            <div v-for=\"(item, index) in spell\" :key=\"index\">\r\n                                <div v-if=\"item.status == 'Bắt đầu'\">\r\n                                    <strong>ĐỢT: </strong> {{ item.name }}\r\n                                </div>\r\n\r\n                            </div>\r\n\r\n\r\n\r\n\r\n                        </div>\r\n                        <div class=\"col-6 mt-3\">\r\n                            <div>\r\n                                <strong>GPA: </strong>\r\n                                <input class=\"gpa\" require type=\"text\" style=\"width: 50px;\" v-model=\"gpa\">\r\n                            </div>\r\n                            <div>\r\n                                <strong>ĐIỂM RÈN LUYỆN: </strong>\r\n                                <input class=\"drl\" require type=\"text\" style=\"width: 50px;\" v-model=\"drl\">\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"modal-body\">\r\n                        <div class=\"table-assess d-flex\">\r\n                            <table class=\"table\">\r\n                                <thead>\r\n                                    <tr>\r\n                                        <th scope=\"col\" class=\"col-1\">STT</th>\r\n                                        <th scope=\"col\" class=\"col-10\">Tiêu chí đánh giá</th>\r\n                                    </tr>\r\n                                </thead>\r\n                                <tbody>\r\n                                    <tr>\r\n                                        <th scope=\"row\">I</th>\r\n                                        <td style=\"font-weight: bold\">Đạo đức tốt</td>\r\n                                    </tr>\r\n                                    <tr v-for=\"(item, index) in standards\" :key=\"index\">\r\n                                        <template v-if=\"item.categoryName=='Đạo đức tốt' && item.status=='Hiện'\">\r\n                                            <th scope=\"row\"></th>\r\n                                            <td>{{ item.contentStandard }}</td>\r\n                                        </template>\r\n                                        \r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">II</th>\r\n                                        <td style=\"font-weight: bold\">\r\n                                            Học tập tốt\r\n                                        </td>\r\n                                    </tr>\r\n                                    <tr v-for=\"(item, index) in standards\" :key=\"index\">\r\n                                        <template v-if=\"item.categoryName=='Học tập tốt' && item.status=='Hiện'\">\r\n                                            <th scope=\"row\"></th>\r\n                                            <td>{{ item.contentStandard }}</td>\r\n                                        </template>\r\n                                        \r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">III</th>\r\n                                        <td style=\"font-weight: bold\">Thể lực tốt</td>\r\n                                    </tr>\r\n                                    <tr v-for=\"(item, index) in standards\" :key=\"index\">\r\n                                        <template v-if=\"item.categoryName=='Thể lực tốt' && item.status=='Hiện'\">\r\n                                            <th scope=\"row\"></th>\r\n                                            <td>{{ item.contentStandard }}</td>\r\n                                        </template>\r\n                                        \r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">IV</th>\r\n                                        <td style=\"font-weight: bold\">Tình nguyện tốt</td>\r\n                                    </tr>\r\n                                    <tr v-for=\"(item, index) in standards\" :key=\"index\">\r\n                                        <template v-if=\"item.categoryName=='Tình nguyện tốt' && item.status=='Hiện'\">\r\n                                            <th scope=\"row\"></th>\r\n                                            <td>{{ item.contentStandard }}</td>\r\n                                        </template>\r\n                                        \r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">V</th>\r\n                                        <td style=\"font-weight: bold\">Hội nhập tốt</td>\r\n                                    </tr>\r\n                                    <tr v-for=\"(item, index) in standards\" :key=\"index\">\r\n                                        <template v-if=\"item.categoryName=='Hội nhập tốt' && item.status=='Hiện'\">\r\n                                            <th scope=\"row\"></th>\r\n                                            <td>{{ item.contentStandard }}</td>\r\n                                        </template>\r\n                                        \r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\"></th>\r\n                                        <td style=\"font-weight: bold\">Các hoạt động tiêu biểu khác:</td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <td colspan=\"3\">\r\n                                            <textarea placeholder=\"Điền các hoạt động đã tham gia (ghi ngày cụ thể)...\"\r\n                                                name=\"\" id=\"\" cols=\"10\" rows=\"3\" class=\"form-control\" v-model=\"more\"></textarea>\r\n                                        </td>\r\n                                    </tr>\r\n                                </tbody>\r\n                            </table>\r\n\r\n                        </div>\r\n                    </div>\r\n                    <div v-if=\"st.rule=='TEACHER'\" class=\"modal-footer\" style=\"jus: center;width: 100%;\">\r\n                        <button type=\"button\" class=\"btn btn-success\" data-bs-dismiss=\"modal\" \r\n                            >\r\n                            Đạt\r\n                        </button>\r\n                        <button type=\"button\" class=\"btn btn-danger\" data-bs-dismiss=\"modal\" >\r\n                            Chưa đạt\r\n                        </button>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n// import config from \"@/config\";\r\nimport { mapGetters } from \"vuex\";\r\nimport axios from \"axios\";\r\nexport default {\r\n    name: \"ViewDetail\",\r\n    \r\n    data() {\r\n        return {\r\n            form: []\r\n        };\r\n    },\r\n    created() {\r\n        this.getForm()\r\n    },\r\n    computed: {\r\n        ...mapGetters({ st: \"getAccount\" }),\r\n    },\r\n    methods: {\r\n        async getForm() {\r\n            const token = localStorage.getItem(\"token\");\r\n            const data1 = await axios.get(\"http://localhost:3000/student/get-form-id\", {\r\n                headers: {\r\n                    Authorization: \"Bearer \" + token,\r\n                }\r\n            })\r\n\r\n            const data_form = data1.data.arr\r\n\r\n            \r\n\r\n\r\n            const arr_form =data_form.map((item, index) => {\r\n                return {\r\n                    stt: index + 1,\r\n                    id: item.studentId._id,\r\n                    username: item.studentId.username,\r\n                    name: item.studentId.name,\r\n                    major: item.major,\r\n                    course: item.course,\r\n                    drl: item.drl,\r\n                    gpa: item.gpa,\r\n                    result: item.result,\r\n                    spellname: item.spellname,\r\n                    standard: item.standard,\r\n                    created: item.createdAt\r\n                }\r\n            })\r\n            this.form = arr_form;\r\n        }\r\n\r\n        \r\n\r\n    }\r\n    \r\n\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.table-assess {\r\n    box-shadow: rgba(99, 99, 99, 0.2) 0px 2px 8px 0px;\r\n}\r\n</style> \r\n"]},"metadata":{},"sourceType":"module"}