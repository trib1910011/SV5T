dex\" >\r\n                                   <template v-if=\"item.rule == 'TEACHER'\">\r\n                                       <input type=\"checkbox\" name=\"check\" id=\"check\" style=\"height: 20px;width: 20px;\"> {{ item.name }}\r\n                                   </template>\r\n                                </div>\r\n                                </div>\r\n                                \r\n                            </div>\r\n                        </div>\r\n                    </form>\r\n                    <table class=\"table\">\r\n                                <thead>\r\n                                    <tr>\r\n                                        <th scope=\"col\" class=\"col-1\">STT</th>\r\n                                        <th scope=\"col\" class=\"col-10\">Tiêu chí đánh giá</th>\r\n                                        <th scope=\"col\" class=\"col-1\">Đạt</th>\r\n                                    </tr>\r\n                                </thead>\r\n                                <tbody>\r\n                                    <tr>\r\n                                        <th scope=\"row\">I</th>\r\n                                        <td style=\"font-weight: bold\">Đạo đức tốt</td>\r\n                                    </tr>\r\n                                    <tr v-for=\"(item, index) in standards\" :key=\"index\">\r\n                                        <template v-if=\"item.categoryName == 'Đạo đức tốt' && item.status == 'Hiện'\">\r\n                                            <th scope=\"row\"></th>\r\n                                            <td>{{ item.contentStandard }}</td>\r\n                                            <td><input type=\"checkbox\" style=\"height: 20px;width: 20px;\"\r\n                                                    class=\"standard_check_box\"\r\n                                                    v-bind:data-standard=\"(`DD${item.contentStandard}`)\"></td>\r\n              