{"ast":null,"code":"import axios from \"axios\";\nimport config from \"@/config/index\";\nimport { mapGetters } from \"vuex\";\nexport default {\n  name: \"CreateStandard\",\n\n  data() {\n    return {\n      categoryCode: \"\",\n      categoryName: \"\",\n      standard: []\n    };\n  },\n\n  methods: {\n    async handleCreateCategory() {\n      const token = localStorage.getItem(\"token\");\n      const res = await axios.post(`${config.domain}/ministry/create-category`, {\n        categoryCode: this.categoryCode.trim(),\n        categoryName: this.categoryName.trim()\n      }, {\n        headers: {\n          Authorization: \"Bearer \" + token\n        }\n      });\n      this.$store.commit(\"SET_TOAST\", {\n        message: res.data.message,\n        isSuccess: res.data.status\n      });\n\n      if (res.data.status) {\n        this.$store.commit(\"ADD_CATEGORY_CURRENT\", {\n          _id: res.data.newCategory._id,\n          categoryCode: this.categoryCode,\n          categoryName: this.categoryName\n        });\n        this.categoryCode = '';\n        this.categoryName = '';\n      }\n\n      this.$parent.showCategory();\n    }\n\n  },\n  computed: { ...mapGetters({\n      standard: \"getCategories\"\n    })\n  }\n};","map":{"version":3,"mappings":"AA0EA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,gBAAnB;AACA,SAASC,UAAT,QAA2B,MAA3B;AACA,eAAe;EACXC,IAAI,EAAE,gBADK;;EAEXC,IAAI,GAAG;IACH,OAAO;MACHC,YAAY,EAAE,EADX;MAEHC,YAAY,EAAE,EAFX;MAGHC,QAAQ,EAAE;IAHP,CAAP;EAKH,CARU;;EASXC,OAAO,EAAE;IACL,MAAMC,oBAAN,GAA6B;MACzB,MAAMC,KAAI,GAAIC,YAAY,CAACC,OAAb,CAAqB,OAArB,CAAd;MACA,MAAMC,GAAE,GAAI,MAAMb,KAAK,CAACc,IAAN,CACb,GAAEb,MAAM,CAACc,MAAO,2BADH,EAEd;QACIV,YAAY,EAAE,KAAKA,YAAL,CAAkBW,IAAlB,EADlB;QAEIV,YAAY,EAAE,KAAKA,YAAL,CAAkBU,IAAlB;MAFlB,CAFc,EAMd;QACIC,OAAO,EAAE;UACLC,aAAa,EAAE,YAAYR;QADtB;MADb,CANc,CAAlB;MAaA,KAAKS,MAAL,CAAYC,MAAZ,CAAmB,WAAnB,EAAgC;QAC5BC,OAAO,EAAER,GAAG,CAACT,IAAJ,CAASiB,OADU;QAE5BC,SAAS,EAAET,GAAG,CAACT,IAAJ,CAASmB;MAFQ,CAAhC;;MAKA,IAAIV,GAAG,CAA