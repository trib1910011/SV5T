{"ast":null,"code":"import { mapGetters } from \"vuex\";\nimport axios from \"axios\";\nimport config from \"@/config/index.js\";\nexport default {\n  name: \"EditInforAccount\",\n\n  data() {\n    return {\n      password: \"\"\n    };\n  },\n\n  methods: {\n    selectTypeAccount(event) {\n      const type = event.target.value;\n      const studentClass = document.querySelector(\".student-class-update\");\n\n      if (type == 2) {\n        studentClass.classList.remove(\"d-none\");\n      } else {\n        if (!studentClass.classList.contains(\"d-none\")) studentClass.classList.add(\"d-none\");\n      }\n    },\n\n    async handleUpdateAccount() {\n      try {\n        const token = localStorage.getItem(\"token\");\n        const res = await axios.patch(`${config.domain}/admin/update-account`, {\n          accountId: this.accountUpdate._id,\n          name: this.accountUpdate.name.trim(),\n          password: this.password.trim()\n        }, {\n          headers: {\n            Authorization: \"Bearer \" + token\n          }\n        });\n        this.$store.commit(\"SET_TOAST\", {\n          message: res.data.message,\n          isSuccess: res.data.status\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    }\n\n  },\n  computed: mapGetters({\n    accountUpdate: \"getAccountUpdate\"\n  })\n};","map":{"version":3,"mappings":"AAuFA,SAASA,UAAT,QAA2B,MAA3B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,mBAAnB;AACA,eAAe;EACXC,IAAI,EAAE,kBADK;;EAEXC,IAAI,GAAG;IACH,OAAO;MACHC,QAAQ,EAAE;IADP,CAAP;EAGH,CANU;;EAOXC,OAAO,EAAE;IACLC,iBAAiB,CAACC,KAAD,EAAQ;MACrB,MAAMC,IAAG,GAAID,KAAK,CAACE,MAAN,CAAaC,KAA1B;MACA,MAAMC,YAAW,GAAIC,QAAQ,CAACC,aAAT,CACjB,uBADiB,CAArB;;MAGA,IAAIL,IAAG,IAAK,CAAZ,EAAe;QACXG,YAAY,CAACG,SAAb,CAAuBC,MAAvB,CAA8B,QAA9B;MACJ,CAFA,MAEO;QACH,IAAI,CAACJ,YAAY,CAACG,SAAb,CAAuBE,QAAvB,CAAgC,QAAhC,CAAL,EACIL,YAAY,CAACG,SAAb,CAAuBG,GAAvB,CAA2B,QAA3B;MACR;IACH,CAZI;;IAaL,MAAMC,mBAAN,GAA4B;MACxB,IAAI;QACA,MAAMC,KAAI,GAAIC,YAAY,CAACC,OAA