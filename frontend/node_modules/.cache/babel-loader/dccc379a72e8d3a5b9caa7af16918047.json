{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapGetters } from \"vuex\"; // import config from \"@/config\";\n\nimport axios from \"axios\";\nexport default {\n  name: \"CreateForm\",\n  computed: { ...mapGetters({\n      student: \"getAccount\"\n    })\n  },\n\n  data() {\n    return {\n      spell: [],\n      gpa: \"\",\n      drl: \"\"\n    };\n  },\n\n  created() {\n    this.showSpell();\n  },\n\n  methods: {\n    async showSpell() {\n      const data = await axios.get(\"http://localhost:3000/spell/get-all-spell\");\n      const arr_data = data.data.findSpell;\n      const arr_result = arr_data.map((item, index) => {\n        return {\n          id: item._id,\n          stt: index + 1,\n          spellname: item.spellname,\n          name: item.name,\n          start: item.start,\n          end: item.end,\n          status: item.status ? 'Bắt đầu' : 'Kết thúc'\n        };\n      });\n      this.spell = arr_result;\n    },\n\n    handleAssess() {\n      var arr_standard = [];\n      let DOM_Checkbox_standart = document.querySelectorAll(\".standard_check_box\");\n      DOM_Checkbox_standart.forEach(item => {\n        if (item.checked) {\n          arr_standard.push(item.dataset.standard);\n        }\n      }); //Sau khi thoát khỏi form\n\n      DOM_Checkbox_standart.forEach(item => {\n        item.checked = false;\n      });\n    },\n\n    cancelForm() {\n      let DOM_Checkbox_standart = document.querySelectorAll(\".standard_check_box\");\n      DOM_Checkbox_standart.forEach(item => {\n        item.checked = false;\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":";AAiXA,SAASA,UAAT,QAA2B,MAA3B,C,CACA;;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAI,EAAE,YADK;EAEXC,QAAQ,EAAE,EACN,GAAGH,UAAU,CAAC;MAAEI,OAAO,EAAE;IAAX,CAAD;EADP,CAFC;;EAMXC,IAAI,GAAG;IACH,OAAO;MACHC,KAAK,EAAE,EADJ;MAEHC,GAAG,EAAE,EAFF;MAGHC,GAAG,EAAE;IAHF,CAAP;EAKH,CAZU;;EAaXC,OAAO,GAAG;IACN,KAAKC,SAAL;EACH,CAfU;;EAgBXC,OAAO,EAAE;IACL,MAAMD,SAAN,GAAkB;MACd,MAAML,IAAG,GAAI,MAAMJ,KAAK,CAACW,GAAN,CAAU,2CAAV,CAAnB;MAEA,MAAMC,QAAO,GAAIR,IAAI,CAACA,IAAL,CAAUS,SAA3B;MAEA,MAAMC,UAAS,GAAIF,QAAQ,CAACG,GAAT,CAAa,CAACC,IAAD,EAAOC,KAAP,KAAiB;QAC7C,OAAO;UACHC,EAAE,EAAEF,IAAI,CAACG,GADN;UAEHC,GAAG,EAAEH,KAAI,GAAI,CAFV;UAGHI,SAAS,EAAEL,IAAI,CAACK,SAHb;UAIHpB,IAAI,EAAEe,IAAI,CAACf,IAJR;UAKHqB,KAAK,EAAEN,IAAI,CAACM,KALT;UAMHC,GAAG,EAAEP,IAAI,CAACO,GANP;UAOHC,MAAM,EAAER,IAAI,CAACQ,MAAL,GAAc,SAAd,GAA0B;QAP/B,CAAP;MAWH,CAZkB,CAAnB;MAcA,KAAKnB,KAAL,GAAaS,UAAb;IACH,CArBI;;IAsBLW,YAAY,GAAG;MACX,IAAIC,YAAW,GAAI,EAAnB;MACA,IAAIC,qBAAoB,GAAIC,QAAQ,CAACC,gBAAT,CAA0B,qBAA1B,CAA5B;MAEAF,qBAAqB,CAACG,OAAtB,CAA+Bd,IAAD,IAAU;QACpC,IAAIA,IAAI,CAACe,OAAT,EAAkB;UACdL,YAAY,CAACM,IAAb,CAAkBhB,IAAI,CAACiB,OAAL,CAAaC,QAA/B;QACJ;MAEH,CALD,EAJW,CAaX;;MACAP,qBAAqB,CAACG,OAAtB,CAA+Bd,IAAD,IAAU;QACpCA,IAAI,CAACe,OAAL,GAAe,KAAf;MACH,CAFD;IAIH,CAxCI;;IAyCLI,UAAU,GAAG;MACT,IAAIR,qBAAoB,GAAIC,QAAQ,CAACC,gBAAT,CAA0B,qBAA1B,CAA5B;MACAF,qBAAqB,CAACG,OAAtB,CAA+Bd,IAAD,IAAU;QACpCA,IAAI,CAACe,OAAL,GAAe,KAAf;MACH,CAFD;IAIJ;;EA/CK;AAhBE,CAAf","names":["mapGetters","axios","name","computed","student","data","spell","gpa","drl","created","showSpell","methods","get","arr_data","findSpell","arr_result","map","item","index","id","_id","stt","spellname","start","end","status","handleAssess","arr_standard","DOM_Checkbox_standart","document","querySelectorAll","forEach","checked","push","dataset","standard","cancelForm"],"sourceRoot":"","sources":["D:\\SV5T\\frontend\\src\\components\\Student\\Form\\CreateForm.vue"],"sourcesContent":["<template>\r\n    <div class=\"assess-result\">\r\n        <!-- Button trigger modal -->\r\n\r\n        <!-- Modal -->\r\n        <div class=\"modal fade\" id=\"AssessResultModal\" tabindex=\"-1\" aria-labelledby=\"ModalLabel\" aria-hidden=\"true\">\r\n            <div class=\"modal-dialog modal-dialog-centered\" style=\"min-width: 800px !important;\">\r\n                <div class=\"modal-content\">\r\n                    <div class=\"modal-header\">\r\n                        <h5 class=\"modal-title fw-bold\" id=\"ModalLabel\" style=\"text-align: center;width: 100%;\">\r\n                            ĐƠN ĐĂNG KÝ XÉT SINH VIÊN 5 TỐT\r\n                        </h5>\r\n                        <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"\r\n                            @click=\"cancelForm\"></button>\r\n                    </div>\r\n                    <div class=\"col-12 d-flex mx-5\">\r\n                        <div class=\"col-6\">\r\n                            <strong>HỌ VÀ TÊN: </strong> {{ student.name }}<br>\r\n                            <strong>MSSV: </strong> {{ student.username }}<br>\r\n                            <strong>NGÀNH: </strong> {{ student.major }}<br>\r\n                            <strong>KHÓA: </strong> {{ student.course }}<br>\r\n                            <div v-for=\"(item, index) in spell\" :key=\"index\">\r\n                                <div v-if=\"item.status == 'Bắt đầu'\">\r\n                                    <strong>ĐỢT: </strong> {{ item.name }}\r\n                                </div>\r\n\r\n                            </div>\r\n\r\n\r\n\r\n\r\n                        </div>\r\n                        <div class=\"col-6 mt-3\">\r\n                            <div>\r\n                                <strong>GPA: </strong>\r\n                                <input class=\"gpa\" require type=\"text\" style=\"width: 50px;\"  v-model=\"gpa\">\r\n                            </div>\r\n                            <div>\r\n                                <strong>ĐIỂM RÈN LUYỆN: </strong>\r\n                                <input class=\"drl\" require type=\"text\" style=\"width: 50px;\"  v-model=\"name\">\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"modal-body\">\r\n                        <div class=\"table-assess d-flex\">\r\n                            <table class=\"table\">\r\n                                <thead>\r\n                                    <tr>\r\n                                        <th scope=\"col\" class=\"col-1\">STT</th>\r\n                                        <th scope=\"col\" class=\"col-10\">Tiêu chí đánh giá</th>\r\n                                        <th scope=\"col\" class=\"col-1\">Đạt</th>\r\n                                    </tr>\r\n                                </thead>\r\n                                <tbody>\r\n                                    <tr>\r\n                                        <th scope=\"row\">I</th>\r\n                                        <td style=\"font-weight: bold\">Đạo đức tốt</td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">I.1</th>\r\n                                        <td>\r\n                                            Điểm rèn luyện trung bình cộng của năm học đạt từ 80 điểm trở lên.\r\n                                        </td>\r\n                                        <td><input type=\"checkbox\" class=\"standard_check_box\" style=\"width:20px;height:20px\"\r\n                                                data-standard=\"Điểm rèn luyện trung bình cộng của năm học đạt từ 80 điểm trở lên.\">\r\n                                        </td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">I.2</th>\r\n                                        <td>\r\n                                            Không vi phạm pháp luật và các quy chế, nội quy của nhà trường, quy định của địa\r\n                                            phương và cộng đồng.\r\n                                        </td>\r\n                                        <td><input type=\"checkbox\" class=\"standard_check_box\" style=\"width:20px;height:20px\"\r\n                                                data-standard=\"Không vi phạm pháp luật và các quy chế, nội quy của nhà trường, quy định của địa\r\n                                            phương và cộng đồng.\"></td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">I.3</th>\r\n                                        <td>\r\n                                            Và đạt thêm 1 trong các tiêu chí sau:\r\n                                        </td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">I.3.1</th>\r\n                                        <td>-Tham gia cuộc thi tìm hiểu về các môn khoa học chính trị, tư tưởng Hồ Chí Minh\r\n                                            từ cấp Khoa, Viện, Bộ môn (gọi tắt là cấp Khoa), Liên chi hội sinh viên trở lên\r\n                                            tổ chức.</td>\r\n                                        <td><input type=\"checkbox\" class=\"standard_check_box\" style=\"width:20px;height:20px\"\r\n                                                data-standard=\"-Tham gia cuộc thi tìm hiểu về các môn khoa học chính trị, tư tưởng Hồ Chí Minh\r\n                                            từ cấp Khoa, Viện, Bộ môn (gọi tắt là cấp Khoa), Liên chi hội sinh viên trở lên\r\n                                            tổ chức.\"></td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">I.3.2</th>\r\n                                        <td>-Tham gia cuộc thi tìm hiểu về lịch sử, văn hóa, con người Việt Nam từ cấp Khoa,\r\n                                            Liên chi hội sinh viên trở lên tổ chức.</td>\r\n                                        <td><input type=\"checkbox\" class=\"standard_check_box\" style=\"width:20px;height:20px\"\r\n                                                data-standard=\"-Tham gia cuộc thi tìm hiểu về lịch sử, văn hóa, con người Việt Nam từ cấp Khoa,\r\n                                            Liên chi hội sinh viên trở lên tổ chức.\"></td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">I.3.3</th>\r\n                                        <td>-Tham gia các hoạt động giáo dục lòng yêu nước, tinh thần tự hào dân tộc: tham\r\n                                            quan các di tích lịch sử, bảo tàng; thăm hỏi mẹ Việt Nam anh hùng; “Lễ thắp nến\r\n                                            tri ân các anh hùng liệt sĩ”; hội thi hát ca khúc truyền thống; hoạt động hướng\r\n                                            về biên giới, hải đảo,…</td>\r\n                                        <td><input type=\"checkbox\" class=\"standard_check_box\" style=\"width:20px;height:20px\"\r\n                                                data-standard=\"-Tham gia các hoạt động giáo dục lòng yêu nước, tinh thần tự hào dân tộc: tham\r\n                                            quan các di tích lịch sử, bảo tàng; thăm hỏi mẹ Việt Nam anh hùng; “Lễ thắp nến\r\n                                            tri ân các anh hùng liệt sĩ”; hội thi hát ca khúc truyền thống; hoạt động hướng\r\n                                            về biên giới, hải đảo,…\"></td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">I.3.4</th>\r\n                                        <td>-Tham gia hoạt động giáo dục đạo đức, lối sống văn hóa cho sinh viên: tham gia\r\n                                            Đội Cờ đỏ, An ninh xung kích; tham gia hoạt động tuyên truyền về pháp luật, kiến\r\n                                            thức phòng tránh các tệ nạn xã hội;…</td>\r\n                                        <td><input type=\"checkbox\" class=\"standard_check_box\" style=\"width:20px;height:20px\"\r\n                                                data-standard=\"-Tham gia hoạt động giáo dục đạo đức, lối sống văn hóa cho sinh viên: tham gia\r\n                                            Đội Cờ đỏ, An ninh xung kích; tham gia hoạt động tuyên truyền về pháp luật, kiến\r\n                                            thức phòng tránh các tệ nạn xã hội;…\"></td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">II</th>\r\n                                        <td style=\"font-weight: bold\">\r\n                                            Học tập tốt\r\n                                        </td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\"></th>\r\n                                        <td>Đạt 1 trong các tiêu chuẩn sau:</td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">II.1</th>\r\n                                        <td>Điểm trung bình cộng của học kỳ 1 và 2 trong năm học đạt từ 2.8 trở lên và không\r\n                                            bị nợ học phần trong năm học.</td>\r\n                                        <td><input type=\"checkbox\" class=\"standard_check_box\" style=\"width:20px;height:20px\"\r\n                                                data-standard=\"Điểm trung bình cộng của học kỳ 1 và 2 trong năm học đạt từ 2.8 trở lên và không\r\n                                            bị nợ học phần trong năm học.\"></td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">II.2</th>\r\n                                        <td>\r\n                                            Điểm trung bình cộng của học kỳ 1 và 2 trong năm học đạt từ 2.6 trở lên, học kỳ\r\n                                            2 phải có kết quả học tập lớn hơn học kỳ 1 và không bị nợ học phần trong năm\r\n                                            học. Số tín chỉ học ở học kỳ 2 phải bằng hoặc lớn hơn số tín chỉ học kỳ 1.\r\n                                        </td>\r\n                                        <td><input type=\"checkbox\" class=\"standard_check_box\" style=\"width:20px;height:20px\"\r\n                                                data-standard=\"Điểm trung bình cộng của học kỳ 1 và 2 trong năm học đạt từ 2.6 trở lên, học kỳ\r\n                                            2 phải có kết quả học tập lớn hơn học kỳ 1 và không bị nợ học phần trong năm\r\n                                            học. Số tín chỉ học ở học kỳ 2 phải bằng hoặc lớn hơn số tín chỉ học kỳ 1.\">\r\n                                        </td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">III</th>\r\n                                        <td style=\"font-weight: bold\">Thể lực tốt</td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\"></th>\r\n                                        <td>Đạt 1 trong các tiêu chuẩn sau:</td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">III.1</th>\r\n                                        <td>\r\n                                            Trong năm học phải học ít nhất 01 học phần Giáo dục thể chất (học phần có rèn\r\n                                            luyện thể lực) và đạt điểm B trở lên.\r\n                                        </td>\r\n                                        <td><input type=\"checkbox\" class=\"standard_check_box\" style=\"width:20px;height:20px\"\r\n                                                data-standard=\"Trong năm học phải học ít nhất 01 học phần Giáo dục thể chất (học phần có rèn\r\n                                            luyện thể lực) và đạt điểm B trở lên.\"></td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">III.2</th>\r\n                                        <td>Tham gia ít nhất 01 hoạt động rèn luyện thể lực (thể dục, thể thao, văn nghệ,…)\r\n                                            do cấp Khoa, Liên chi hội trở lên tổ chức.</td>\r\n                                        <td><input type=\"checkbox\" class=\"standard_check_box\" style=\"width:20px;height:20px\"\r\n                                                data-standard=\"Tham gia ít nhất 01 hoạt động rèn luyện thể lực (thể dục, thể thao, văn nghệ,…)\r\n                                            do cấp Khoa, Liên chi hội trở lên tổ chức.\"></td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">IV</th>\r\n                                        <td style=\"font-weight: bold\">Tình nguyện tốt</td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">IV.1</th>\r\n                                        <td>Được khen thưởng trong hoạt động tình nguyện.</td>\r\n                                        <td><input type=\"checkbox\" class=\"standard_check_box\" style=\"width:20px;height:20px\"\r\n                                                data-standard=\"Được khen thưởng trong hoạt động tình nguyện.\"></td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">IV.2</th>\r\n                                        <td>\r\n                                            Tham gia ít nhất 03 hoạt động tình nguyện.\r\n                                        </td>\r\n                                        <td><input type=\"checkbox\" class=\"standard_check_box\" style=\"width:20px;height:20px\"\r\n                                                data-standard=\"Tham gia ít nhất 03 hoạt động tình nguyện.\"></td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">IV.3</th>\r\n                                        <td>Tham gia ít nhất 02 lần Hiến máu tình nguyện.</td>\r\n                                        <td><input type=\"checkbox\" class=\"standard_check_box\" style=\"width:20px;height:20px\"\r\n                                                data-standard=\"Tham gia ít nhất 02 lần Hiến máu tình nguyện.\"></td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">V</th>\r\n                                        <td style=\"font-weight: bold\">Hội nhập tốt</td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\"></th>\r\n                                        <td>Đạt 2 trong 3 tiêu chuẩn sau:</td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">V.1</th>\r\n                                        <td>Ngoại ngữ: đạt 01 trong 02 tiêu chí sau:</td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">V.1.1</th>\r\n                                        <td>\r\n                                            Được cấp giấy khen tham gia cuộc thi ngoại ngữ cấp trường trở lên.\r\n                                        </td>\r\n                                        <td><input type=\"checkbox\" class=\"standard_check_box\" style=\"width:20px;height:20px\"\r\n                                                data-standard=\"Được cấp giấy khen tham gia cuộc thi ngoại ngữ cấp trường trở lên.\">\r\n                                        </td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">V.1.2</th>\r\n                                        <td>Đạt chứng chỉ ngoại ngữ theo bảng quy đổi</td>\r\n                                        <td><input type=\"checkbox\" class=\"standard_check_box\" style=\"width:20px;height:20px\"\r\n                                                data-standard=\"Đạt chứng chỉ ngoại ngữ theo bảng quy đổi\"></td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">V.2</th>\r\n                                        <td>Tin học: đạt 01 trong 04 tiêu chí sau:</td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">V.2.1</th>\r\n                                        <td>\r\n                                            Đạt chứng chỉ tin học cơ bản trở lên;\r\n                                        </td>\r\n                                        <td><input type=\"checkbox\" class=\"standard_check_box\" style=\"width:20px;height:20px\"\r\n                                                data-standard=\"Đạt chứng chỉ tin học cơ bản trở lên;\"></td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">V.2.2</th>\r\n                                        <td>Điểm trung bình của 2 học phần tin học căn bản từ điểm B trở lên;</td>\r\n                                        <td><input type=\"checkbox\" class=\"standard_check_box\" style=\"width:20px;height:20px\"\r\n                                                data-standard=\"Điểm trung bình của 2 học phần tin học căn bản từ điểm B trở lên;\">\r\n                                        </td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">V.2.3</th>\r\n                                        <td>Được cấp giấy khen hoặc giấy chứng nhận tham gia cuộc thi tin học cấp khoa,\r\n                                            trường trở lên.</td>\r\n                                        <td><input type=\"checkbox\" class=\"standard_check_box\" style=\"width:20px;height:20px\"\r\n                                                data-standard=\"Được cấp giấy khen hoặc giấy chứng nhận tham gia cuộc thi tin học cấp khoa,\r\n                                            trường trở lên.\"></td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <th scope=\"row\">V.2.4</th>\r\n                                        <td>Trong năm học có học ít nhất 01 học phần hoặc khóa học về tin học, công nghệ\r\n                                            thông tin và điểm trung bình học phần đạt từ B hoặc khóa học đạt từ 7.0 điểm\r\n                                            (loại khá) trở lên.</td>\r\n                                        <td><input type=\"checkbox\" class=\"standard_check_box\" style=\"width:20px;height:20px\"\r\n                                                data-standard=\"Trong năm học có học ít nhất 01 học phần hoặc khóa học về tin học, công nghệ\r\n                                            thông tin và điểm trung bình học phần đạt từ B hoặc khóa học đạt từ 7.0 điểm\r\n                           