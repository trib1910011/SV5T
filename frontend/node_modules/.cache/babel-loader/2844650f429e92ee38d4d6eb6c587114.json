{"ast":null,"code":"import StudentFilter from \"@/components/StudentFilter/StudentFilter.vue\";\nimport axios from \"axios\";\nimport config from \"@/config/index.js\";\nimport { mapActions, mapGetters } from \"vuex\";\nexport default {\n  name: \"StudentsList\",\n  components: {\n    StudentFilter\n  },\n  computed: mapGetters({\n    teacher: \"getTeacherDetail\",\n    currentList: \"getCurrentList\"\n  }),\n  methods: mapActions([\"setTeacherDetail\"]),\n\n  async created() {\n    // get teacherId query\n    const teacherId = this.$route.params.teacherId;\n    const token = localStorage.getItem(\"token\"); // set teacher detail\n\n    this.setTeacherDetail(teacherId);\n    const res = await axios.get(`${config.domain}/ministry/get-student-managed`, {\n      params: {\n        teacherId\n      },\n      headers: {\n        Authorization: \"Bearer \" + token\n      }\n    });\n    this.$store.commit(\"SET_CLASS_NAME\", res.data.classes);\n    this.$store.commit(\"SET_STUDENT_LIST\", res.data.result);\n    this.$store.commit(\"SET_CURRENT_LIST\", res.data.result);\n  }\n\n};","map":{"version":3,"mappings":"AAiDA,OAAOA,aAAP,MAA0B,8CAA1B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,mBAAnB;AACA,SAASC,UAAT,EAAqBC,UAArB,QAAuC,MAAvC;AACA,eAAe;EACXC,IAAI,EAAE,cADK;EAEXC,UAAU,EAAE;IAAEN;EAAF,CAFD;EAGXO,QAAQ,EAAEH,UAAU,CAAC;IACjBI,OAAO,EAAE,kBADQ;IAEjBC,WAAW,EAAE;EAFI,CAAD,CAHT;EAOXC,OAAO,EAAEP,UAAU,CAAC,CAAC,kBAAD,CAAD,CAPR;;EAQX,MAAMQ,OAAN,GAAgB;IACZ;IACA,MAAMC,SAAQ,GAAI,KAAKC,MAAL,CAAYC,MAAZ,CAAmBF,SAArC;IAEA,MAAMG,KAAI,GAAIC,YAAY,CAACC,OAAb,CAAqB,OAArB,CAAd,CAJY,CAMZ;;IACA,KAAKC,gBAAL,CAAsBN,SAAtB;IAEA,MAAMO,GAAE,GAAI,MAAMlB,KAAK,CAACmB,GAAN,CACb,GAAElB,MAAM,CAACmB,MAAO,+BADH,EAEd;MACIP,MAAM,EAAE;QACJF;MADI,CADZ;MAIIU,OAAO,EAAE;QACLC,aAAa,EAAE,YAAYR;MADtB;IAJb,CAFc,CAAlB;IAWA,KAAKS,MAAL,CAAYC,MAAZ,CAAmB,gBAAnB,EAAqCN,GAAG,CAACO,IAAJ,CAASC,OAA9C;IACA,KAAKH,MAAL,CAAYC,MAAZ,CAAmB,kBAAnB,EAAuCN,GAAG,CAACO,IAAJ,CAASE,MAAhD;IACA,KAAKJ,MAAL,CAAYC,MAAZ,CAAmB,kBAAnB,EAAuCN,GAAG,CAACO,IAAJ,CAASE,MAAhD;EACH;;AA/BU,CAAf","names":["StudentFilter","axios","config","mapActions","mapGetters","name","components","computed","teacher","currentList","methods","created","teacherId","$route","params","token","localStorage","getItem","setTeacherDetail","res","get","domain","headers","Authorization","$store","commit","data","classes","result"],"sourceRoot":"","sources":["/Users/thiencao/Desktop/Niên luận ngành KTPM/Code/internship-management/frontend/src/components/Ministry/StudentsManagement/StudentsList.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <div class=\"student-list d-flex animate__fadeIn animate__animated\">\r\n            <div class=\"header-list m-0 p-2 pt-3 col-6\" style=\"color: #555555\">\r\n                <div class=\"d-flex align-items-center\">\r\n                    <h2 class=\"me-3 fw-bold\">\r\n                        Danh sách sinh viên đang quản lý\r\n                    </h2>\r\n                    <img\r\n                        src=\"https://cdn-icons-png.flaticon.com/512/4426/4426381.png\"\r\n                        alt=\"Lọc sinh viên\"\r\n                        data-bs-toggle=\"modal\"\r\n                        data-bs-target=\"#StudentFilter\"\r\n                        class=\"filter-icon\"\r\n                    />\r\n                </div>\r\n\r\n                <div class=\"line mt-3 mb-2\"></div>\r\n            </div>\r\n        </div>\r\n\r\n        <table class=\"table table-striped text-center\">\r\n            <thead>\r\n                <tr>\r\n                    <th scope=\"col\">#</th>\r\n                    <th scope=\"col\">Mã số</th>\r\n                    <th scope=\"col\">Họ và tên</th>\r\n                    <th scope=\"col\">Lớp</th>\r\n                    <th scope=\"col\">Điện thoại</th>\r\n                    <th scope=\"col\">Email</th>\r\n                </tr>\r\n            </thead>\r\n\r\n            <tbody style=\"overflow-y: scroll\">\r\n                <tr v-for=\"(student, index) in currentList\" :key=\"index\">\r\n                    <th scope=\"row\">{{ index + 1 }}</th>\r\n                    <td>{{ student.username }}</td>\r\n                    <td>{{ student.name }}</td>\r\n                    <td>{{ student.class }}</td>\r\n                    <td>{{ student.phoneNumber }}</td>\r\n                    <td>{{ student.email }}</td>\r\n                </tr>\r\n            </tbody>\r\n        </table>\r\n        <StudentFilter />\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport StudentFilter from \"@/components/StudentFilter/StudentFilter.vue\";\r\nimport axios from \"axios\";\r\nimport config from \"@/config/index.js\";\r\nimport { mapActions, mapGetters } from \"vuex\";\r\nexport default {\r\n    name: \"StudentsList\",\r\n    components: { StudentFilter },\r\n    computed: mapGetters({\r\n        teacher: \"getTeacherDetail\",\r\n        currentList: \"getCurrentList\",\r\n    }),\r\n    methods: mapActions([\"setTeacherDetail\"]),\r\n    async created() {\r\n        // get teacherId query\r\n        const teacherId = this.$route.params.teacherId;\r\n\r\n        const token = localStorage.getItem(\"token\");\r\n\r\n        // set teacher detail\r\n        this.setTeacherDetail(teacherId);\r\n\r\n        const res = await axios.get(\r\n            `${config.domain}/ministry/get-student-managed`,\r\n            {\r\n                params: {\r\n                    teacherId,\r\n                },\r\n                headers: {\r\n                    Authorization: \"Bearer \" + token,\r\n                },\r\n            }\r\n        );\r\n        this.$store.commit(\"SET_CLASS_NAME\", res.data.classes);\r\n        this.$store.commit(\"SET_STUDENT_LIST\", res.data.result);\r\n        this.$store.commit(\"SET_CURRENT_LIST\", res.data.result);\r\n    },\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"]},"metadata":{},"sourceType":"module"}