{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n// import config from \"@/config\";\nimport axios from \"axios\";\nexport default {\n  name: \"CreateForm\",\n\n  data() {\n    return {\n      student: [],\n      spell: [],\n      standardMoral: [],\n      standardStudy: [],\n      standardHealth: [],\n      standardVolunteer: [],\n      standardIntegrate: [],\n      gpa: \"\",\n      drl: \"\",\n      more: \"\"\n    };\n  },\n\n  created() {\n    this.showSpell();\n    this.getStandard();\n    this.showInforStudent();\n  },\n\n  methods: {\n    async showSpell() {\n      const data = await axios.get(\"http://localhost:3000/spell/get-all-spell\");\n      const arr_data = data.data.findSpell;\n      const arr_result = arr_data.map((item, index) => {\n        return {\n          id: item._id,\n          stt: index + 1,\n          spellname: item.spellname,\n          name: item.name,\n          start: item.start,\n          end: item.end,\n          status: item.status ? 'Bắt đầu' : 'Kết thúc'\n        };\n      });\n      this.spell = arr_result;\n    },\n\n    addToArr(inputString) {\n      console.log(inputString);\n      const code_split = inputString.split(\"-\")[0];\n      const string_split = inputString.split(\"-\")[1];\n\n      switch (code_split) {\n        case 'DD':\n          this.standardMoral.push(string_split);\n          break;\n\n        case 'HT':\n          this.standardStudy.push(string_split);\n\n        case 'default':\n          break;\n      }\n\n      console.log(this.standardMoral);\n    },\n\n    async handleAssess() {\n      var arr_standard = [];\n      let DOM_Checkbox_standart = document.querySelectorAll(\".standard_check_box\");\n      DOM_Checkbox_standart.forEach(item => {\n        if (item.checked) {\n          arr_standard.push(item.dataset.standard);\n        }\n      });\n      console.log(arr_standard);\n      let spell_name_choose = \"\";\n      this.spell.forEach(item => {\n        if (item.status === \"Bắt đầu\") {\n          spell_name_choose = item.name;\n        }\n      });\n      const ob = {\n        studentId: this.student[0].id,\n        username: this.username,\n        name: this.name,\n        major: this.student[0].major,\n        course: this.student[0].course,\n        spellname: spell_name_choose,\n        gpa: this.gpa,\n        drl: this.drl,\n        standard: arr_standard,\n        more: this.more\n      };\n      const token = localStorage.getItem(\"token\");\n      const res = await axios.post(\"http://localhost:3000/student/register-form\", ob, {\n        headers: {\n          Authorization: \"Bearer \" + token\n        }\n      });\n      this.$store.commit(\"SET_TOAST\", {\n        message: res.data.message,\n        isSuccess: res.data.status\n      });\n      this.$parent.showForm(); // .then((data) => {\n      //     console.log(data)\n      // }).catch((e) => {\n      //     console.log(e)\n      // })\n      //Sau khi thoát khỏi form\n\n      DOM_Checkbox_standart.forEach(item => {\n        item.checked = false;\n      });\n      this.gpa = \"\";\n      this.drl = \"\";\n      this.more = \"\";\n    },\n\n    cancelForm() {\n      let DOM_Checkbox_standart = document.querySelectorAll(\".standard_check_box\");\n      DOM_Checkbox_standart.forEach(item => {\n        item.checked = false;\n      });\n      this.gpa = \"\";\n      this.drl = \"\";\n      this.more = \"\";\n    },\n\n    async getStandard() {\n      const token = localStorage.getItem(\"token\");\n      const data = await axios.get(\"http://localhost:3000/student/get-standard\", {\n        headers: {\n          Authorization: \"Bearer \" + token\n        }\n      });\n      const arr_data = data.data.standards;\n      const arr_result = arr_data.map((item, index) => {\n        return {\n          id: item._id,\n          stt: index + 1,\n          codeStandard: item.codeStandard,\n          categoryName: item.categoryName,\n          contentStandard: item.contentStandard,\n          status: item.status ? 'Hiện' : 'Ẩn',\n          check: item.check\n        };\n      });\n      this.standards = arr_result;\n    },\n\n    async showInforStudent() {\n      const token = localStorage.getItem(\"token\");\n      const data = await axios.post(\"http://localhost:3000/student/get-student-infor\", {\n        token\n      }, {\n        headers: {\n          Authorization: \"Bearer \" + token\n        }\n      });\n      const arr_data = data.data.result;\n      const arr_result = [arr_data].map((item, index) => {\n        return {\n          id: item.studentId,\n          urlAvatar: item.urlAvatar,\n          stt: index + 1,\n          username: item.username,\n          name: item.name,\n          major: item.major,\n          course: item.course\n        };\n      });\n      this.student = arr_result;\n    }\n\n  }\n};","map":{"version":3,"mappings":";AAsKA;AACA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAI,EAAE,YADK;;EAGXC,IAAI,GAAG;IACH,OAAO;MACHC,OAAO,EAAE,EADN;MAEHC,KAAK,EAAE,EAFJ;MAGHC,aAAa,EAAE,EAHZ;MAIHC,aAAa,EAAE,EAJZ;MAKHC,cAAc,EAAE,EALb;MAMHC,iBAAiB,EAAE,EANhB;MAOHC,iBAAiB,EAAE,EAPhB;MAQHC,GAAG,EAAE,EARF;MASHC,GAAG,EAAE,EATF;MAUHC,IAAI,EAAE;IAVH,CAAP;EAYH,CAhBU;;EAiBXC,OAAO,GAAG;IACN,KAAKC,SAAL;IACA,KAAKC,WAAL;IACA,KAAKC,gBAAL;EACH,CArBU;;EAsBXC,OAAO,EAAE;IACL,MAAMH,SAAN,GAAkB;MACd,MAAMZ,IAAG,GAAI,MAAMF,KAAK,CAACkB,GAAN,CAAU,2CAAV,CAAnB;MAGA,MAAMC,QAAO,GAAIjB,IAAI,CAACA,IAAL,CAAUkB,SAA3B;MAEA,MAAMC,UAAS,GAAIF,QAAQ,CAACG,GAAT,CAAa,CAACC,IAAD,EAAOC,KAAP,KAAiB;QAC7C,OAAO;UACHC,EAAE,EAAEF,IAAI,CAACG,GADN;UAEHC,GAAG,EAAEH,KAAI,GAAI,CAFV;UAGHI,SAAS,EAAEL,IAAI,CAACK,SAHb;UAIH3B,IAAI,EAAEsB,IAAI,CAACtB,IAJR;UAKH4B,KAAK,EAAEN,IAAI,CAACM,KALT;UAMHC,GAAG,EAAEP,IAAI,CAACO,GANP;UAOHC,MAAM,EAAER,IAAI,CAACQ,MAAL,GAAc,SAAd,GAA0B;QAP/B,CAAP;MAWH,CAZkB,CAAnB;MAcA,KAAK3B,KAAL,GAAaiB,UAAb;IAEH,CAvBI;;IAwBLW,QAAO,CAAGC,WAAH,EAAgB;MACnBC,OAAO,CAACC,GAAR,CAAYF,WAAZ;MACA,MAAMG,UAAS,GAAIH,WAAW,CAACI,KAAZ,CAAkB,GAAlB,EAAuB,CAAvB,CAAnB;MACA,MAAMC,YAAW,GAAIL,WAAW,CAACI