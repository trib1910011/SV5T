{"ast":null,"code":"import AvatarComponent from \"./AvatarComponent.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"InforStudent\",\n  components: {\n    AvatarComponent\n  },\n\n  created() {\n    this.showInforStudent();\n  },\n\n  methods: {\n    async showInforStudent() {\n      const token = localStorage.getItem(\"token\");\n      const data = await axios.get(\"http://localhost:3000/student/get-student-infor\", {\n        headers: {\n          Authorization: \"Bearer \" + token\n        }\n      });\n      const arr_data = data.data.findStudent;\n      const arr_result = arr_data.map(item => {\n        return {\n          id: item._id,\n          username: item.username,\n          name: item.categoryname,\n          major: item.major,\n          course: item.course\n        };\n      });\n      this.student = arr_result;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAwBA,OAAOA,eAAP,MAA4B,uBAA5B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAI,EAAE,cADK;EAEXC,UAAU,EAAE;IAAEH;EAAF,CAFD;;EAGXI,OAAO,GAAG;IACN,KAAKC,gBAAL;EACH,CALU;;EAMXC,OAAO,EAAE;IACL,MAAMD,gBAAN,GAAyB;MACrB,MAAME,KAAI,GAAIC,YAAY,CAACC,OAAb,CAAqB,OAArB,CAAd;MACA,MAAMC,IAAG,GAAI,MAAMT,KAAK,CAACU,GAAN,CAAU,iDAAV,EACf;QACIC,OAAO,EAAE;UACLC,aAAa,EAAE,YAAYN;QADtB;MADb,CADe,CAAnB;MASA,MAAMO,QAAO,GAAIJ,IAAI,CAACA,IAAL,CAAUK,WAA3B;MAEA,MAAMC,UAAS,GAAIF,QAAQ,CAACG,GAAT,CAAcC,IAAD,IAAU;QAEtC,OAAO;UACHC,EAAE,EAAED,IAAI,CAACE,GADN;UAEHC,QAAQ,EAAEH,IAAI,CAACG,QAFZ;UAGHnB,IAAI,EAAEgB,IAAI,CAACI,YAHR;UAIHC,KAAK,EAAEL,IAAI,CAACK,KAJT;UAKHC,MAAM,EAAEN,IAAI,CAACM;QALV,CAAP;MAQH,CAVkB,CAAnB;MAWA,KAAKC,OAAL,GAAeT,UAAf;IAEH;;EA3BI;AANE,CAAf","names":["AvatarComponent","axios","name","components","created","showInforStudent","methods","token","localStorage","getItem","data","get","headers","Authorization","arr_data","findStudent","arr_result","map","item","id","_id","username","categoryname","major","course","student"],"sourceRoot":"","sources":["D:\\SV5T\\frontend\\src\\components\\GlobalCompon