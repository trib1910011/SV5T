{"ast":null,"code":"import axios from \"axios\";\nimport config from \"@/config/index\";\nexport default {\n  name: \"CreateStandard\",\n\n  data() {\n    return {\n      categories: [],\n      codeStandard: \"\",\n      contentStandard: \"\",\n      chooseStandard: \"\"\n    };\n  },\n\n  created() {\n    this.getCategory();\n  },\n\n  methods: {\n    async handleCreateStandard() {\n      const check = document.querySelector(\".form-select select-type-category\").value;\n      console.log(check);\n      const token = localStorage.getItem(\"token\");\n      const res = await axios.post(`${config.domain}/ministry/create-standard`, {\n        codeStandard: this.codeStandard.trim(),\n        contentStandard: this.contentStandard.trim()\n      }, {\n        headers: {\n          Authorization: \"Bearer \" + token\n        }\n      });\n      this.$store.commit(\"SET_TOAST\", {\n        message: res.data.message,\n        isSuccess: res.data.status\n      });\n\n      if (res.data.status) {\n        this.$store.commit(\"ADD_STANDARD_CURRENT\", {\n          _id: res.data.newStandard._id,\n          codeStandard: this.codeStandard,\n          nameStandard: this.nameStandard\n        });\n        this.codeStandard = '';\n        this.nameStandard = '';\n      }\n\n      this.$parent.showStandard();\n    },\n\n    async getCategory() {\n      const token = localStorage.getItem(\"token\");\n      const data = await axios.get(\"http://localhost:3000/ministry/get-all-categories\", {\n        headers: {\n          Authorization: \"Bearer \" + token\n        }\n      });\n      const arr_data = data.data.categories;\n      const arr_result = arr_data.map(item => {\n        return {\n          categoryName: item.categoryName\n        };\n      });\n      this.categories = arr_result;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoDA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,gBAAnB;AACA,eAAe;EACXC,IAAI,EAAE,gBADK;;EAEXC,IAAI,GAAG;IACH,OAAO;MACHC,UAAU,EAAE,EADT;MAEHC,YAAY,E