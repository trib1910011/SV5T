{"ast":null,"code":"import axios from \"axios\"; // import config from \"@/config/index.js\";\n\nimport TitleStructure from \"../../components/GlobalComponent/TitleStructure.vue\"; // import EditInforAccount from \"../../components/Admin/ManageAccount/EditInforAccount.vue\";\n\nimport CreateSpell from \"../../components/Ministry/ManageSpell/CreateSpell.vue\"; // import { mapGetters } from \"vuex\";\n\nexport default {\n  name: \"AdminManage\",\n  components: {\n    TitleStructure,\n    CreateSpell\n  },\n\n  data() {\n    return {\n      spell: []\n    };\n  },\n\n  created() {\n    this.showSpell();\n  },\n\n  methods: {\n    async showSpell() {\n      const data = await axios.get(\"http://localhost:3000/spell/get-all-spell\");\n      const arr_data = data.data.findSpell;\n      const arr_result = arr_data.map((item, index) => {\n        return {\n          stt: index + 1,\n          spellname: item.spellname,\n          name: item.name,\n          start: item.start,\n          end: item.end,\n          status: item.status ? 'Bắt đầu' : 'Kết thúc'\n        };\n      });\n      this.spell = arr_result;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6FA,OAAOA,KAAP,MAAkB,OAAlB,C,CACA;;AACA,OAAOC,cAAP,MAA2B,qDAA3B,C,CACA;;AACA,OAAOC,WAAP,MAAwB,uDAAxB,C,CACA;;AAEA,eAAe;EACXC,IAAI,EAAE,aADK;EAEXC,UAAU,EAAE;IAAEH,cAAF;IAAkBC;EAAlB,CAFD;;EAGXG,IAAI,GAAG;IACH,OAAO;MACHC,KAAK,EAAE;IADJ,CAAP;EAGH,CAPU;;EAQXC,OAAM,GAAK;IACP,KAAKC,SAAL;EACH,CAVU;;EAWXC,OAAO,EAAE;IACL,MAAMD,SAAN,GAAkB;MACd,MAAMH,IAAG,GAAI,MAAML,KAAK,CAACU,GAAN,CAAU,2CAAV,CAAnB;MAEA,MAAMC,QAAO,GAAIN,IAAI,CAACA,IAAL,CAAUO,SAA3B;MAEA,MAAMC,UAAS,GAAIF,QAAQ,CAACG,GAAT,CAAa,CAACC,IAAD,EAAOC,KAAP,KAAe;QAEvC,OAAO;UACPC,GAAG,EAAED,KAAI,GAAI,CADN;UAEPE,SAAS,EAAEH,IAAI,CAACG,SAFT;UAGPf,IAAI,EAAEY,IAAI,CAACZ,IAHJ;UAIPgB,KAAK,EAAEJ,IAAI,CAACI,KAJL;UAKPC,GAAG,EAAEL,IAAI,CAACK,GALH;UAMPC,MAAM,EAAEN,IAAI,CAACM,MAAL,GAAY,SAAZ,GAAsB;QANvB,CAAP;MAUP,CAZkB,CAAnB;MAcA,KAAKf,KAAL,GAAaO,UAAb;IAEJ;;EAtBK;AAXE,CAAf","names":["axios