n      const res = await axios.post(\"http://localhost:3000/student/register-form\", ob, {\n        headers: {\n          Authorization: \"Bearer \" + token\n        }\n      });\n      this.$store.commit(\"SET_TOAST\", {\n        message: res.data.message,\n        isSuccess: res.data.status\n      });\n      this.$parent.showForm(); // .then((data) => {\n      //     console.log(data)\n      // }).catch((e) => {\n      //     console.log(e)\n      // })\n      //Sau khi thoát khỏi form\n\n      DOM_Checkbox_standart.forEach(item => {\n        item.checked = false;\n      });\n      this.gpa = \"\";\n      this.drl = \"\";\n      this.more = \"\";\n    },\n\n    cancelForm() {\n      let DOM_Checkbox_standart = document.querySelectorAll(\".standard_check_box\");\n      DOM_Checkbox_standart.forEach(item => {\n        item.checked = false;\n      });\n      this.gpa = \"\";\n      this.drl = \"\";\n      this.more = \"\";\n    },\n\n    async getStandard() {\n      const token = localStorage.getItem(\"token\");\n      const data = await axios.get(\"http://localhost:3000/student/get-standard\", {\n        headers: {\n          Authorization: \"Bearer \" + token\n        }\n      });\n      const arr_data = data.data.standards;\n      const arr_result = arr_data.map((item, index) => {\n        return {\n          id: item._id,\n          stt: index + 1,\n          codeStandard: item.codeStandard,\n          categoryName: item.categoryName,\n          contentStandard: item.contentStandard,\n          status: item.status ? 'Hiện' : 'Ẩn',\n          check: item.check\n        };\n      });\n      this.standards = arr_result;\n    },\n\n    async showInforStudent() {\n      const token = localStorage.getItem(\"token\");\n      const data = await axios.post(\"http://localhost:3000/student/get-student-infor\", {\n        token\n      }, {\n        headers: {\n          Authorization: \"Bearer \" + token\n        }\n      });\n      const arr_data = data.data.result;\n      const arr_result = [arr_data].map((item, index) => {\n        return {\n          id: item.studentId,\n          urlAvatar: item.urlAvatar,\n          stt: index + 1,\n          username: item.username,\n          name: item.name,\n          major: item.major,\n          course: item.course\n        };\n      });\n      this.student = arr_result;\n    }\n\n  }\n};","map":{"version":3,"mappings":";AAiLA;AACA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAI,EAAE,YADK;EAEXC,KAAK,EAAE,CAAC,YAAD,CAFI;;EAGXC,IAAI,GAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EADP;MAEHC,SAAS,EAAE;IAFR,CAAP;EAIH,CARU;;EASXC,OAAO,GAAG;IACN;IACA;IACA,KAAKC,gBAAL;EACH,CAbU;;EAcXC,OAAO,EAAE;IACL;IACA;IAGA;IAEA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IAGA;IAEA;IAEA;IACA,MAAMC,YAAN,GAAqB;MACjB,IAAIC,YAAW,GAAI,EAAnB;MACA,IAAIC,qBAAoB,GAAIC,QAAQ,CAACC,gBAAT,CAA0B,qBAA1B,CAA5B;MAEAF,qBAAqB,CAACG,OAAtB,CAA+BC,IAAD,IAAU;QACpC,IAAIA,IAAI,CAACC,OAAT,EAAkB;UACdN,YAAY,CAACO,IAAb,CAAkBF,IAAI,CAACG,OAAL,CAAaC,QAA/B;QACJ;MACH,CAJD,EAJiB,CAUjB;MACA;MACA;MACA;MACA;MACA;;MAEA,MAAMC,EAAC,GAAI;QACPC,SAAS,EAAE,KAAKC,OAAL,CAAa,CAAb,EAAgBC,EADpB;QAEPC,YAAY,EAAE,KAAKF,OAAL,CAAa,CAAb,EAAgBE,YAFvB;QAGPC,KAAK,EAAE,KAAKH,OAAL,CAAa,CAAb,EAAgBG,KAHhB;QAIPC,MAAM,EAAE,KAAKJ,OAAL,CAAa,CAAb,EAAgBI,MAJjB;QAKPC,SAAS,EAAE,KAAKC,UAAL,CAAgB3B,IALpB;QAMP4B,GAAG,EAAE,KAAKA,GANH;QAOPC,GAAG,EAAE,KAAKA,GAPH;QAQPX,QAAQ,EAAET,YARH;QASPqB,IAAI,EAAE,KAAKA,IATJ;QAUPC,WAAW,EAAE,KAAKA;MAVX,CAAX;MAaAC,OAAO,CAACC,GAAR,CAAYd,EAAZ;MACA,MAAMe,KAAI,GAAIC,YAAY,CAACC,OAAb,CAAqB,OAArB,CAAd;MAEA,MAAMC,GAAE,GAAI,MAAMtC,KAAK,CAACuC,IAAN,CAAW,6CAAX,EACdnB,EADc,EACV;QACJoB,OAAO,EAAE;UACLC,aAAa,EAAE,YAAYN;QADtB;MADL,CADU,CAAlB;MAOA,KAAKO,MAAL,CAAYC,MAAZ,CAAmB,WAAnB,EAAgC;QAC5BC,OAAO,EAAEN,GAAG,CAACnC,IAAJ,CAASyC,OADU;QAE5BC,SAAS,EAAEP,GAAG,CAACnC,IAAJ,CAAS2C;MAFQ,CAAhC;MAIA,KAAKC,OAAL,CAAaC,QAAb,GA5CiB,CA6CjB;MACA;MACA;MACA;MACA;MAEA;;MACArC,qBAAqB,CAACG,OAAtB,CAA+BC,IAAD,IAAU;QACpCA,IAAI,CAACC,OAAL,GAAe,KAAf;MACH,CAFD;MAGA,KAAKa,GAAL,GAAW,EAAX;MACA,KAAKC,GAAL,GAAW,EAAX;MA