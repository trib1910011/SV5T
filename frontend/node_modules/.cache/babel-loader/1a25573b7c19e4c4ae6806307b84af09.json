{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport TitleStructure from \"../../components/GlobalComponent/TitleStructure.vue\";\nexport default {\n  name: \"FormManageMini\",\n  components: {\n    TitleStructure\n  },\n\n  created() {\n    this.showForm();\n  },\n\n  data() {\n    return {\n      form: []\n    };\n  },\n\n  mounted() {\n    document.querySelectorAll(\".type__item\")[0].classList.add(\"active\");\n    const typeItems = document.querySelectorAll(\".type__item\");\n    [...typeItems].forEach(function (typeItem) {\n      typeItem.addEventListener(\"click\", handleBorderItem);\n    });\n\n    function handleBorderItem(event) {\n      [...typeItems].forEach(typeItem => {\n        typeItem.classList.remove(\"active\");\n      });\n      event.target.classList.add(\"active\");\n    }\n  },\n\n  methods: {\n    handleFilter(event) {\n      const filter = event.target.id;\n\n      if (filter == \"teacher\") {\n        this.$store.commit(\"SET_ACCOUNT_CURRENT\", this.accounts.filter(account => account.rule == \"TEACHER\"));\n      } else if (filter == \"student\") {\n        this.$store.commit(\"SET_ACCOUNT_CURRENT\", this.accounts.filter(account => account.rule == \"STUDENT\"));\n      } else if (filter == \"ministry\") {\n        this.$store.commit(\"SET_ACCOUNT_CURRENT\", this.accounts.filter(account => account.rule == \"MINISTRY\"));\n      } else if (filter == \"all\") {\n        this.$store.commit(\"SET_ACCOUNT_CURRENT\", this.accounts);\n      }\n    },\n\n    async showForm() {\n      const token = localStorage.getItem(\"token\");\n      const data1 = await axios.get(\"http://localhost:3000/ministry/get-form\", {\n        headers: {\n          Authorization: \"Bearer \" + token\n        }\n      });\n      const data_form = data1.data.arr;\n      const arr_result = [];\n      data_form.forEach((item, index) => {\n        let ob = {\n          stt: index + 1,\n          id: item._id,\n          username: item.studentId.username,\n          name: item.studentId.name,\n          major: item.major,\n          course: item.course,\n          drl: item.drl,\n          gpa: item.gpa,\n          result: item.result,\n          spellname: item.spellname,\n          standard: item.standard,\n          created: item.createdAt\n        };\n        arr_result.push(ob);\n      });\n      this.form = arr_result;\n    }\n\n  }\n};","map":{"version":3,"mappings":";AA4DA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,cAAP,MAA2B,qDAA3B;AAGA,eAAe;EACXC,IAAI,EAAE,gBADK;EAEXC,UAAU,EAAE;IAAEF;EAAF,CAFD;;EAGXG,OAAO,GAAG;IACN,KAAKC,QAAL;EACH,CALU;;EAMXC,IAAI,GAAG;IACH,OAAO;MACHC,IAAI,EAAE;IADH,CAAP;EAGH,CAVU;;EAWXC,OAAO,GAAG;IACNC,QAAQ,CAACC,gBAAT,CAA0B,aAA1B,EAAyC,CAAzC,EAA4CC,SAA5C,CAAsDC,GAAtD,CAA0D,QAA1D;IAEA,MAAMC,SAAQ,GAAIJ,QAAQ,CAACC,gBAAT,CAA0B,aAA1B,CAAlB;IAEA,CAAC,GAAGG,SAAJ,EAAeC,OAAf,CAAuB,UAAUC,QAAV,EAAoB;MACvCA,QAAQ,CAACC,gBAAT,CAA0B,OAA1B,EAAmCC,gBAAnC;IACH,CAFD;;IAIA,SAASA,gBAAT,CAA0BC,KAA1B,EAAiC;MAC7B,CAAC,GAAGL,SAAJ,EAAeC,OAAf,CAAwBC,QAAD,IAAc;QACjCA,QAAQ,CAACJ,SAAT,CAAmBQ,MAAnB,CAA0B,QAA1B;MACH,CAFD;MAGAD,KAAK,CAACE,MAAN,CAAaT,SAAb,CAAuBC,GAAvB,CAA2B,QAA3B;IACJ;EACH,CA1BU;;EA2BXS,OAAO,EAAE;IACLC,YAAY,CAACJ,KAAD,EAAQ;MAChB,MAAMK,MAAK,GAAIL,KAAK,CAACE,MAAN,CAAaI,EAA5B;;MACA,IAAID,MAAK,IAAK,SAAd,EAAyB;QACrB,KAAKE,MAAL,CAAYC,MAAZ,CACI,qBADJ,EAEI,KAAKC,QAAL,CAAcJ,MAAd,CAAsBK,OAAD,IAAaA,OAAO,CAACC,IAAR,IAAgB,SAAlD,CAFJ;MAIJ,CALA,MAKO,IAAIN,MAAK,IAAK,SAAd,EAAyB;QAC5B,KAAKE,MAAL,CAAYC,MAAZ,CACI,qBADJ,EAEI,KAAKC,QAAL,CAAcJ,MAAd,CAAsBK,OAAD,IAAaA,OAAO,CAACC,IAAR,IAAgB,SAAlD,CAFJ;MAIJ,CALO,MAKA,IAAIN,MAAK,IAAK,UAAd,EAA0B;QAC7B,KAAKE,MAAL,CAAYC,MAAZ,CACI,qBADJ,EAEI,KAAKC,QAAL,CAAcJ,MAAd,CAAsBK,OAAD,IAAaA,OAAO,CAACC,IAAR,IAAgB,UAAlD,CAFJ;MAIJ,CALO,MAKA,IAAIN,MAAK,IAAK,KAAd,EAAqB;QACxB,KAAKE,MAAL,CAAYC,MAAZ,CAAmB,qBAAnB,EAA0C,KAAKC,QAA/C;MACJ;IACH,CArBI;;IAsBL,MAAMtB,QAAN,GAAiB;MACb,MAAMyB,KAAI,GAAIC,YAAY,CAACC,OAAb,CAAqB,OAArB,CAAd;MACA,MAAMC,KAAI,GAAI,MAAMjC,KAAK,CAACkC,GAAN,CAAU,yCAAV,EAAqD