a) {\r\n                const ob = {\r\n                    drl: data.drl,\r\n                    gpa: data.gpa,\r\n                    result: data.result,\r\n                    spellname: data.spellname,\r\n                    standard: data.standard,\r\n                    more: data.more,\r\n                    created: data.createdAt,\r\n                    count_check: data.count_check\r\n                }\r\n                console.log(ob)\r\n\r\n\r\n            }\r\n\r\n\r\n\r\n            \r\n        },\r\n\r\n\r\n\r\n    },\r\n\r\n\r\n\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.table-assess {\r\n    box-shadow: rgba(99, 99, 99, 0.2) 0px 2px 8px 0px;\r\n}\r\n</style> \r\n"]},"metadata":{},"sourceType":"module"}