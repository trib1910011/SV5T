{"ast":null,"code":"import InforStudent from \"@/components/GlobalComponent/InforStudent.vue\";\nimport TitleStructure from \"../../components/GlobalComponent/TitleStructure.vue\";\nimport axios from \"axios\";\nimport CreateForm from \"@/components/Student/Form/CreateForm.vue\";\nexport default {\n  name: \"FormRegister\",\n  components: {\n    TitleStructure,\n    InforStudent,\n    CreateForm\n  },\n\n  data() {\n    return {\n      spell: [],\n      empty_check: \"\",\n      form: [],\n      student: []\n    };\n  },\n\n  created() {\n    this.showSpell(), this.showForm();\n  },\n\n  methods: {\n    async showSpell() {\n      const data = await axios.get(\"http://localhost:3000/spell/get-all-spell\");\n      const arr_data = data.data.findSpell;\n      const arr_result = arr_data.map((item, index) => {\n        return {\n          id: item._id,\n          stt: index + 1,\n          spellname: item.spellname,\n          name: item.name,\n          start: item.start,\n          end: item.end,\n          status: item.status ? 'Bắt đầu' : 'Kết thúc'\n        };\n      });\n      var count_empty = 0;\n      arr_result.forEach(item => {\n        if (item.status === \"Kết thúc\") {\n          count_empty++;\n        }\n      });\n\n      if (count_empty === arr_result.length) {\n        this.empty_check = \"CHƯA ĐẾN ĐỢT ĐĂNG KÝ\";\n      }\n\n      this.spell = arr_result;\n    },\n\n    async showForm() {\n      const token = localStorage.getItem(\"token\");\n      const data1 = await axios.get(\"http://localhost:3000/student/get-form\", {\n        headers: {\n          Authorization: \"Bearer \" + token\n        }\n      });\n      const data_form = data1.data.findForm;\n      const arr_form = data_form.map((item, index) => {\n        return {\n          stt: index + 1,\n          id: item._id,\n          username: item.username,\n          name: item.name,\n          major: item.major,\n          course: item.course,\n          drl: item.drl,\n          gpa: item.gpa,\n          result: item.result,\n          spellname: item.spellname,\n          standard: item.standard,\n          created: item.createdAt\n        };\n      });\n      this.form = arr_form;\n    },\n\n    async showInforStudent() {\n      const token = localStorage.getItem(\"token\");\n      const data = await axios.post(\"http://localhost:3000/student/get-student-infor\", {\n        token\n      }, {\n        headers: {\n          Authorization: \"Bearer \" + token\n        }\n      });\n      const arr_data = data.data.result;\n      const arr_result = [arr_data].map((item, index) => {\n        return {\n          id: item.studentId,\n          urlAvatar: item.urlAvatar,\n          stt: index + 1,\n          username: item.username,\n          name: item.name,\n          major: item.major,\n          course: item.course\n        };\n      });\n      this.student = arr_result;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuGA,OAAOA,YAAP,MAAyB,+CAAzB;AACA,OAAOC,cAAP,MAA2B,qDAA3B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,UAAP,MAAuB,0CAAvB;AAEA,eAAe;EACXC,IAAI,EAAE,cADK;EAEXC,UAAU,EAAE;IAAEJ,cAAF;IAAkBD,YAAlB;IAAgCG;EAAhC,CAFD;;EAGXG,IAAI,GAAG;IACH,OAAO;MACHC,KAAK,EAAE,EADJ;MAEHC,WAAW,EAAE,EAFV;MAGHC,IAAI,EAAE,EAHH;MAIHC,OAAO,EAAE;IAJN,CAAP;EAMH,CAVU;;EAWXC,OAAO,GAAG;IACN,KAAKC,SAAL,IACA,KAAKC,QAAL,EADA;EAEH,CAdU;;EAgBXC,OAAO,EAAE;IACL,MAAMF,SAAN,GAAkB;MACd,MAAMN,IAAG,GAAI,MAAMJ,KAAK,CAACa,GAAN,CAAU,2CAAV,CAAnB;MAEA,MAAMC,QAAO,GAAIV,IAAI,CAACA,IAAL,CAAUW,SAA3B;MAEA,MAAMC,UAAS,GAAIF,QAAQ,CAACG,GAAT,CAAa,CAACC,IAAD,EAAOC,KAAP,KAAiB;QAE7C,OAAO;UACHC,EAAE,EAAEF,IAAI,CAACG,GADN;UAEHC,GAAG,EAAEH,KAAI,GAAI,CAFV;UAGHI,SAAS,EAAEL,IAAI,CAACK,SAHb;UAIHrB,IAAI,EAAEgB,IAAI,CAAChB,IAJR;UAKHsB,KAAK,EAAEN,IAAI,CAACM,KALT;UAMHC,GAAG,EAAEP,IAAI,CAACO,GANP;UAOHC,MAAM,EAAER,IAAI,CAACQ,MAAL,GAAc,SAAd,GAA0B;QAP/B,CAAP;MAWH,CAbkB,CAAnB;MAeA,IAAIC,WAAU,GAAI,CAAlB;MACAX,UAAU,CAACY,OAAX,CAAoBV,IAAD,IAAU;QACzB,IAAIA,IAAI,CAACQ,MAAL,KAAgB,UAApB,EAAgC;UAC5BC,WAAW;QACf;MACH,CAJD;;MAMA,IAA