{"ast":null,"code":"import InforTeacher from \"@/components/GlobalComponent/InforTeacher.vue\";\nimport TitleStructure from \"@/components/GlobalComponent/TitleStructure.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"FormManage\",\n  components: {\n    InforTeacher,\n    TitleStructure\n  },\n\n  data() {\n    return {\n      spell: [],\n      empty_check: \"\",\n      form: []\n    };\n  },\n\n  created() {\n    this.showSpell(), this.showForm();\n  },\n\n  methods: {\n    async showSpell() {\n      const data = await axios.get(\"http://localhost:3000/spell/get-all-spell\");\n      const arr_data = data.data.findSpell;\n      const arr_result = arr_data.map((item, index) => {\n        return {\n          id: item._id,\n          stt: index + 1,\n          spellname: item.spellname,\n          name: item.name,\n          start: item.start,\n          end: item.end,\n          status: item.status ? 'Bắt đầu' : 'Kết thúc'\n        };\n      });\n      var count_empty = 0;\n      arr_result.forEach(item => {\n        if (item.status === \"Kết thúc\") {\n          count_empty++;\n        }\n      });\n\n      if (count_empty === arr_result.length) {\n        this.empty_check = \"CHƯA CÓ THỜI GIAN ĐĂNG KÝ ĐỢT MỚI\";\n      }\n\n      this.spell = arr_result;\n    },\n\n    async showForm() {\n      const token = localStorage.getItem(\"token\");\n      const data1 = await axios.get(\"http://localhost:3000/teachet/get-form\", {\n        headers: {\n          Authorization: \"Bearer \" + token\n        }\n      });\n      console.log(data1);\n      const data_form = data1.data.findForm;\n      const arr_form = data_form.map((item, index) => {\n        return {\n          stt: index + 1,\n          id: item._id,\n          username: item.username,\n          name: item.name,\n          major: item.major,\n          course: item.course,\n          drl: item.drl,\n          gpa: item.gpa,\n          result: item.result,\n          spellname: item.spellname,\n          standard: item.standard,\n          created: item.createdAt\n        };\n      });\n      this.form = arr_form;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+CA,OAAOA,YAAP,MAAyB,+CAAzB;AACA,OAAOC,cAAP,MAA2B,iDAA3B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAI,EAAE,YADK;EAEXC,UAAU,EAAE;IAAEJ,YAAF;IAAgBC;EAAhB,CAFD;;EAGXI,IAAI,GAAG;IACH,OAAO;MACHC,KAAK,EAAE,EADJ;MAEHC,WAAW,EAAE,EAFV;MAGHC,IAAI,EAAE;IAHH,CAAP;EAKH,CATU;;EAUXC,OAAO,GAAG;IACN,KAAKC,SAAL,IACA,KAAKC,QAAL,EADA;EAEH,CAbU;;EAcXC,OAAO,EAAE;IACL,MAAMF,SAAN,GAAkB;MACd,MAAML,IAAG,GAAI,MAAMH,KAAK,CAACW,GAAN,CAAU,2CAAV,CAAnB;MAEA,MAAMC,QAAO,GAAIT,IAAI,CAACA,IAAL,CAAUU,SAA3B;MAEA,MAAMC,UAAS,GAAIF,QAAQ,CAACG,GAAT,CAAa,CAACC,IAAD,EAAOC,KAAP,KAAiB;QAE7C,OAAO;UACHC,EAAE,EAAEF,IAAI,CAACG,GADN;UAEHC,GAAG,EAAEH,KAAI,GAAI,CAFV;UAGHI,SAAS,EAAEL,IAAI,CAACK,SAHb;UAIHpB,IAAI,EAAEe,IAAI,CAACf,IAJR;UAKHqB,KAAK,EAAEN,IAAI,CAACM,KALT;UAMHC,GAAG,EAAEP,IAAI,CAACO,GANP;UAOHC,MAAM,EAAER,IAAI,CAACQ,MAAL,GAAc,SAAd,GAA0B;QAP/B,CAAP;MAWH,CAbkB,CAAnB;MAeA,IAAIC,WAAU,GAAI,CAAlB;MACAX,UAAU,CAACY,OAAX,CAAoBV,IAAD,IAAU;QACzB,IAAIA,IAAI,CAACQ,MAAL,KAAgB,UAApB,EAAgC;UAC5BC,WAAW;QACf;MACH,CAJD;;MAMA,IAAIA,WAAU,KAAMX,UAAU,CAACa,MAA/B,EAAuC;QACnC,KAAKtB,WAAL,GAAmB,mCAAnB;MACJ;;MAEA,KAAKD,KAAL,GAAaU,UAAb;IACH,CAjCI;;IAkCL,MAAML,QAAN,GAAiB;MACb,MAAMmB,KAAI,GAAIC,YAAY,CAACC,OAAb,CAAqB,OAArB,CAAd;MACA,MAAMC,KAAI,GAAI,MAAM/B,KAAK,CAACW,GAAN,CAAU,wCAAV,EAAoD;QACpEqB,OAAO,EAAE;UACLC,aAAa,EAAE,YAAYL;QADtB;MAD2D,CAApD,CAApB;MAMAM,OAAO,CAACC,GAAR,CAAYJ,KAAZ;MAEA,MAAMK,SAAQ,GAAIL,KAAK,CAAC5B,IAAN,CAAWkC,QAA7B;MAGA,MAAMC,QAAO,GAAIF,SAAS,CAACrB,GAAV,CAAc,CAACC,IAAD,EAAOC,KAAP,KAAiB;QAC5C,OAAO;UACHG,GAAG,EAAEH,KAAI,GAAI,CADV;UAEHC,EAAE,EAAEF,IAAI,CAACG,GAFN;UAGHoB,QAAQ,EAAEvB,IAAI,CAACuB,QAHZ;UAIHtC,IAAI,EAAEe,IAAI,CAACf,IAJR;UAKHuC,KAAK,EAAExB,IAAI,CAACwB,KALT;UAMHC,MAAM,EAAEzB,IAAI,CAACyB,MANV;UAOHC,GAAG,EAAE1B,IAAI,CAAC0B,GAPP;UAQHC,GAAG,EAAE3B,IAAI,CAAC2B,GARP;UASHC,MAAM,EAAE5B,IAAI,CAAC4B,MATV;UAUHvB,SAAS,EAAEL,IAAI,CAACK,SAVb;UAWHwB,QAA