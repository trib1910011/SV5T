{"ast":null,"code":"import axios from \"axios\"; // import config from \"@/config/index.js\";\n\nimport TitleStructure from \"../../components/GlobalComponent/TitleStructure.vue\"; // import EditInforAccount from \"../../components/Admin/ManageAccount/EditInforAccount.vue\";\n\nimport CreateSpell from \"../../components/Ministry/ManageSpell/CreateSpell.vue\"; // import { mapGetters } from \"vuex\";\n\nexport default {\n  name: \"AdminManage\",\n  components: {\n    TitleStructure,\n    CreateSpell\n  },\n\n  data() {\n    return {\n      spell: []\n    };\n  },\n\n  created() {\n    this.showSpell();\n    this.accounts = response.data.result;\n    this.$store.commit(\"SET_ACCOUNT_CURRENT\", response.data.result);\n  },\n\n  methods: {\n    async showSpell() {\n      const data = await axios.get(\"http://localhost:3000/spell/get-all-spell\");\n      const arr_data = data.data.findSpell;\n      const arr_result = arr_data.map((item, index) => {\n        return {\n          id: item._id,\n          stt: index + 1,\n          spellname: item.spellname,\n          name: item.name,\n          start: item.start,\n          end: item.end,\n          status: item.status ? 'Bắt đầu' : 'Kết thúc'\n        };\n      });\n      this.spell = arr_result;\n    },\n\n    async handleupdateStatus(id_input, status_input) {\n      let status_get = false;\n\n      if (status_input === 'Bắt đầu') {\n        status_get = true;\n      }\n\n      try {\n        const check_update = await axios.patch(`http://localhost:3000/spell/updateOneSpell/${id_input}`, {\n          status: !status_get\n        });\n\n        if (check_update) {\n          alert('Chuyển đổi trạng thái thành công');\n          this.showSpell();\n        } else {\n          alert('Chuyển đổi trạng thái thất bại');\n        }\n      } catch (e) {\n        console.log(e);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuEA,OAAOA,KAAP,MAAkB,OAAlB,C,CACA;;AACA,OAAOC,cAAP,MAA2B,qDAA3B,C,CACA;;AACA,OAAOC,WAAP,MAAwB,