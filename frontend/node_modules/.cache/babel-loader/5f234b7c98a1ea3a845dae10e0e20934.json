{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport AvatarComponent from \"@/components/GlobalComponent/AvatarComponent.vue\";\nimport TitleStructure from \"@/components/GlobalComponent/TitleStructure.vue\";\nexport default {\n  name: \"CommendStudent\",\n\n  data() {\n    return {\n      form: [],\n      spell: [],\n      choose: 'Tất cả'\n    };\n  },\n\n  components: {\n    AvatarComponent,\n    TitleStructure\n  },\n\n  created() {\n    this.showForm();\n    this.showSpell();\n  },\n\n  methods: {\n    async showSpell() {\n      const data = await axios.get(\"http://localhost:3000/spell/get-all-spell\");\n      const arr_data = data.data.findSpell;\n      const arr_result = arr_data.map((item, index) => {\n        return {\n          id: item._id,\n          stt: index + 1,\n          spellname: item.spellname,\n          name: item.name,\n          start: item.start,\n          end: item.end,\n          allocate: item.allocate,\n          status: item.status ? 'Bắt đầu' : 'Kết thúc'\n        };\n      });\n      this.spell = arr_result;\n    },\n\n    async showForm() {\n      const token = localStorage.getItem(\"token\");\n      const data1 = await axios.get(\"http://localhost:3000/student/get-form\", {\n        headers: {\n          Authorization: \"Bearer \" + token\n        }\n      });\n      const data_form = data1.data.arr;\n      const arr_result = [];\n      data_form.forEach((item, index) => {\n        if (item.result == 'Đạt' && item.spellname == this.choose) {\n          let ob = {\n            stt: index + 1,\n            id: item._id,\n            username: item.studentId.username,\n            name: item.studentId.name,\n            urlAvatar: item.studentId.urlAvatar,\n            major: item.major,\n            course: item.course,\n            drl: item.drl,\n            gpa: item.gpa,\n            result: item.result,\n            spellname: item.spellname,\n            standard: item.standard,\n            created: item.createdAt\n          };\n          arr_result.push(ob);\n        }\n      });\n      this.form = arr_result;\n    },\n\n    SelectSpell() {\n      if (choose_item == \"Tất cả\") {\n        this.showForm();\n      } else {\n        this.showForm();\n        setTimeout(() => {\n          this.form = this.form.filter(item => {\n            return item.spellname == choose_item;\n          });\n          console.log(this.form);\n        }, 1100);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":";AA2BA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,eAAP,MAA4B,kDAA5B;AACA,OAAOC,cAAP,MAA2B,iDAA3B;AACA,eAAe;EACXC,IAAI,EAAE,gBADK;;EAEXC,IAAI,GAAG;IACH,OAAO;MACHC,IAAI,EAAE,EADH;MAEHC,KAAK,EAAE,EAFJ;MAGHC,MAAM,EAAE;IAHL,CAAP;EAKH,CARU;;EASXC,UAAU,EAAE;IAAEP,eAAF;IAAmBC;EAAnB,CATD;;EAUXO,OAAO,GAAG;IACN,KAAKC,QAAL;IACA,KAAKC,SAAL;EACH,CAbU;;EAcXC,OAAO,EAAE;IACL,MAAMD,SAAN,GAAkB;MACd,MAAMP,IAAG,GAAI,MAAMJ,KAAK,CAACa,GAAN,CAAU,2CAAV,CAAnB;MAEA,MAAMC,QAAO,GAAIV,IAAI,CAACA,IAAL,CAAUW,SAA3B;MAEA,MAAMC,UAAS,GAAIF,QAAQ,CAACG,GAAT,CAAa,CAACC,IAAD,EAAOC,KAAP,KAAiB;QAE7C,OAAO;UACHC,EAAE,EAAEF,IAAI,CAACG,GADN;UAEHC,GAAG,EAAEH,KAAI,GAAI,CAFV;UAGHI,SAAS,EAAEL,IAAI,CAACK,SAHb;UAIHpB,IAAI,EAAEe,IAAI,CAACf,IAJR;UAKHqB,KAAK,EAAEN,IAAI,CAACM,KALT;UAMHC,GAAG,EAAEP,IAAI,CAACO,GANP;UAOHC,QAAQ,EAAER,IAAI,CAACQ,QAPZ;UAQHC,MAAM,EAAET,IAAI,CAACS,MAAL,GAAc,SAAd,GAA0B;QAR/B,CAAP;MAUH,CAZkB,CAAnB;MAaA,KAAKrB,KAAL,GAAaU,UAAb;IACH,CApBI;;IAqBL,MAAMN,QAAN,GAAiB;MACb,MAAMkB,KAAI,GAAIC,YAAY,CAACC,OAAb,CAAqB,OAArB,CAAd;MACA,MAAMC,KAAI,GAAI,MAAM/B,KAAK,CAACa,GAAN,CAAU,wCAAV,EAAoD;QACpEmB,OAAO,EAAE;UACLC,aAAa,EAAE,YAAYL;QADtB;MAD2D,CAApD,CAApB;MAMA,MAAMM,SAAQ,GAAIH,KAAK,CAAC3B,IAAN,CAAW+B,GAA7B;MACA,MAAMnB,UAAS,GAAI,EAAnB;MACAkB,SAAS,CAACE,OAAV,CAAkB,CAAClB,IAAD,EAAOC,KAAP,KAAiB;QAC/B,IAAID,IAAI,CAACmB,MAAL,IAAe,KAAf,IAAwBnB,IAAI,CAACK,SAAL,IAAkB,KAAKhB,MAAnD,EAA2D;UACvD,IAAI+B,EAAC,GAAI;YACLhB,GAAG,EAAEH,KAAI,GAAI,CADR;YAELC,EAAE,EAAEF,IAAI,CAACG,GAFJ;YAGLkB,QAAQ,EAAErB,IAAI,CAACsB,SAAL,